package seentao.xhsn.enterprise.followresume.dao.impl;

import java.util.List;

import org.apache.ibatis.session.SqlSession;
import org.mybatis.spring.support.SqlSessionDaoSupport;
import org.springframework.stereotype.Repository;

import seentao.xhsn.bean.HsEtpFollowResumeBO;
import seentao.xhsn.common.DaoUtil;
import seentao.xhsn.enterprise.followresume.dao.IFollowResumeDAO;

@Repository
public class FollowResumeDAOImpl extends SqlSessionDaoSupport implements IFollowResumeDAO {

	private SqlSession sqlSession;
	private DaoUtil daoUtil;

	public void initDao() {
		if (daoUtil == null) {
			sqlSession = sqlSession == null ? getSqlSession() : sqlSession;
			daoUtil = new DaoUtil(sqlSession);
		}
	}

	@Override
	public void insert(HsEtpFollowResumeBO bo) {
		daoUtil.insertBO("seentao.xhsn.bean.mappers.HsEtpFollowResumeBOMapper.insert", bo);
	}

	@Override
	public void update(HsEtpFollowResumeBO bo) {
		daoUtil.updateByPK("seentao.xhsn.bean.mappers.HsEtpFollowResumeBOMapper.updateByPrimaryKey", bo);
	}

	@Override
	public void delete(String id) {
		daoUtil.deleteByPK("seentao.xhsn.bean.mappers.HsEtpFollowResumeBOMapper.deleteByPrimaryKey", id);
	}

	@Override
	public HsEtpFollowResumeBO selectByPrimaryKey(String id) {
		return (HsEtpFollowResumeBO) daoUtil.selectByPK(
				"seentao.xhsn.bean.mappers.HsEtpFollowResumeBOMapper.selectByPrimaryKey", id);
	}

	@Override
	public List<HsEtpFollowResumeBO> getAllByParam(HsEtpFollowResumeBO bo) {
		List<HsEtpFollowResumeBO> list = daoUtil.selectByBOParamT(
				"seentao.xhsn.bean.mappers.HsEtpFollowResumeBOMapper.selectByParam", bo);
		return list;
	}

}