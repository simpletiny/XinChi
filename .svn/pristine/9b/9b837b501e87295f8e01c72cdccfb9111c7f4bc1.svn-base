<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@taglib uri="/struts-tags" prefix="s"%>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>
<html>
<head>
<base href="<%=basePath%>">
<link rel="stylesheet" type="text/css" href="common/css/basic.css" />
<link rel="stylesheet" type="text/css" href="common/css/jlyl.css" />
<script src="common/js/jquery-1.8.1.min.js" type="text/javascript"></script>
<script src="common/js/layer-v1.8.1/layer/layer.min.js"></script>
<script src="person/resume/js/resume.js"></script>
<script type="text/javascript" src="common/js/drag.js"></script>
<script type="text/javascript" src="<%=basePath%>common/js/WdatePicker/WdatePicker.js"></script>
<script type="text/javascript" src="common/js/major_arr.js"></script>
<script src="common/js/city_arr.js" type="text/javascript"></script>
<script src="common/js/funtype_arr.js" type="text/javascript"></script>
<script src="common/js/industry_arr.js" type="text/javascript"></script>
</head>
<body>
<s:if test="loginType==2">
	<div class="transparent"></div>
	<input type="hidden" value="${res_id}" id="hid_res_id" />
	<div id="tanchu" style="display:none;position: absolute;z-index:1000">
		<h4>
			<a href="javascript:void(0)" class="close"></a>
		</h4>
		<div class="tanchu_inner">
			<form action="" method="post" name="myform" id="myform"> 
			<input type="hidden" name="informBO.ent_id"  value="${informBO.ent_id }"/> 
			<%--<input type="hidden" name="informBO.job_id"  value="${releaseJobBO.id }"/>--%>
			<input type="hidden" name="informBO.ent_name"  value="${informBO.ent_name}"/> 
			<%--<input type="hidden" name="informBO.job_name"  value="${releaseJobBO.rel_name }"/>--%>
			<p>公司名称：${informBO.ent_name}</p>
			<p style="color:red">*面试地址：<input type="text" name="informBO.ent_address" value="${informBO.ent_address }" style="width:400px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
			<p style="color:red">*面试岗位：<input type="text" name="informBO.job_name" value="${informBO.job_name }" style="width:400px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
			<p style="color:red">&nbsp;&nbsp;&nbsp;*联系人：<input name="informBO.interviewer" type="text" value="${informBO.interviewer}" style="width:90px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/>&nbsp;&nbsp;&nbsp; *联系电话：<input name="informBO.interviewer_phone" type="text" value="${informBO.interviewer_phone}" style="width:200px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
			<p style="color:red">*需带资料：<input name="informBO.attach" type="text" style="width:350px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);" value="纸质简历2份、毕业证等相关证件" /></p>
			<p style="color:red">*面试时间：<input name="informBO.inform_time" type="text" onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm'})" style="width:200px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
		</form>
		</div>
		<p style="line-height: 30px; margin-left: 125px;">
			<input name="" type="checkbox" value="" style="margin-right: 10px;" />注：如无法参加本次面试，请及时通知我公司，谢谢！
		</p>
		<a href="javascript:void(0)" onclick="sendinform('<%=basePath%>enterprise/sendInforms')"
			style="display: block; width: 190px; height: 46px; line-height: 48px; text-align: center; color: #FFF; background: #fea201; font-size: 16px; border-bottom: 2px solid #c9b387; margin-left: 300px; margin-top: 10px;">立即发送</a>
	</div>
	<div style="display:block;width:600px;z-index:200;height:20px;margin-left:20px;margin-top:20px;position:fixed;">
		<input name="" class="dianji"
			style="width: 88px;float:left; background: #fea201; height: 28px; color: #fdfdfd; font-size: 14px; cursor: pointer;"
			onclick="showInterview()" type="button" value="通知面试" />
	</div>
</s:if>
	
<div style="margin-right:70px;height:20px">
	<a href="#" onclick="PrintNoHdr();" style="float:right">打印</a>
</div>
<!--startprint1-->
<div id="main">
		<h2>${resume.name}</h2>
		<s:if test='resume.baseinfo_path!=null&&resume.baseinfo_path!=""'>
			<s:include value="../../%{resume.baseinfo_path}"></s:include>
		</s:if>
		<s:if test='resume.desire_path!=null&&resume.desire_path!=""'>
			<s:include value="../../%{resume.desire_path}"></s:include>
		</s:if>
		<s:if test='resume.education_path!=null&&resume.education_path!=""'>
			<s:include value="../../%{resume.education_path}"></s:include>
		</s:if>
		<s:if test='resume.evaluate_path!=null&&resume.evaluate_path!=""'>
			<s:include value="../../%{resume.evaluate_path}"></s:include>
		</s:if>
		<s:if test='resume.practice_path!=null&&resume.practice_path!=""'>
			<s:include value="../../%{resume.practice_path}"></s:include>
		</s:if>
		<s:if test='resume.project_path!=null&&resume.project_path!=""'>
			<s:include value="../../%{resume.project_path}"></s:include>
		</s:if>
		<s:if test='resume.skill_path!=null&&resume.skill_path!=""'>
			<s:include value="../../%{resume.skill_path}"></s:include>
		</s:if>
		<s:if test='resume.train_path!=null&&resume.train_path!=""'>
			<s:include value="../../%{resume.train_path}"></s:include>
		</s:if>
		<s:if test='resume.certificate_path!=null&&resume.certificate_path!=""'>
			<s:include value="../../%{resume.certificate_path}"></s:include>
		</s:if>
	<div>
		<h4 style="background:url(common/images/resume/images/xxlj2.png);"></h4>
		<table cellspacing="0px"
			style="width:17cm;border-collapse:collapse; border:0;  text-align:center; margin-left:50px;">
	
			<tr>
				<s:iterator value="pathlist" id="path" >
				<td style="width:3cm" height="45">${path.scoreCatalogBO.score_name}</td>
				</s:iterator>
			</tr>
			<tr>
				<s:iterator value="pathlist" id="path" >
				<td style="width:3cm" height="45" style="color:#059e81;">${path.score}</td>
				</s:iterator>
			</tr>
		</table>
	</div>
</div>
<!--endprint1-->
</body>
</html>