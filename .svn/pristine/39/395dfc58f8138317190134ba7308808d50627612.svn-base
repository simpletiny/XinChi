package seentao.xhsn.enterprise.storeresume.dao.impl;

import java.util.List;

import org.apache.ibatis.session.SqlSession;
import org.mybatis.spring.support.SqlSessionDaoSupport;
import org.springframework.stereotype.Repository;

import seentao.xhsn.bean.HsEtpStoreResumeBO;
import seentao.xhsn.common.DaoUtil;
import seentao.xhsn.enterprise.storeresume.dao.IStoreResumeDAO;


@Repository
public class StoreResumeDAOImpl extends SqlSessionDaoSupport implements IStoreResumeDAO{

	private SqlSession sqlSession;
	private DaoUtil daoUtil;
	public void initDao(){
		if(daoUtil==null){
			sqlSession=sqlSession==null?getSqlSession():sqlSession;
			daoUtil=new DaoUtil(sqlSession);
		}
	}
	
	@Override
	public void insert(HsEtpStoreResumeBO bo) {
		daoUtil.insertBO("seentao.xhsn.bean.mappers.HsEtpStoreResumeBOMapper.insert", bo);
	}

	@Override
	public void update(HsEtpStoreResumeBO bo) {
		daoUtil.updateByPK("seentao.xhsn.bean.mappers.HsEtpStoreResumeBOMapper.updateByPrimaryKey", bo);
	}

	@Override
	public void delete(String id) {
		daoUtil.deleteByPK("seentao.xhsn.bean.mappers.HsEtpStoreResumeBOMapper.deleteByPrimaryKey", id);
	}

	@Override
	public HsEtpStoreResumeBO selectByPrimaryKey(String id) {
		return (HsEtpStoreResumeBO) daoUtil.selectByPK("seentao.xhsn.bean.mappers.HsEtpStoreResumeBOMapper.selectByPrimaryKey", id);
	}

	@Override
	public List<HsEtpStoreResumeBO> getAllByParam(HsEtpStoreResumeBO bo) {
		List<HsEtpStoreResumeBO> list=daoUtil.selectByBOParamT("seentao.xhsn.bean.mappers.HsEtpStoreResumeBOMapper.selectByParam", bo);
		return list;
	}
	
}