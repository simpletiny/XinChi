<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@taglib uri="/struts-tags" prefix="s"%>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<title>新道人才网</title>
<link rel="stylesheet" type="text/css" href="<%=basePath%>common/css/basic.css" />
<link rel="stylesheet" type="text/css" href="<%=basePath%>common/css/page.css" />
<link rel="stylesheet" type="text/css" href="<%=basePath%>enterprise/managejob/css/zwgl.css" />
<script src="<%=basePath%>common/js/jquery-1.8.1.min.js" type="text/javascript"></script>
<script src="<%=basePath%>common/js/layer-v1.8.1/layer/layer.min.js"></script>
<script src="<%=basePath%>common/js/jquery.pagination.js" type="text/javascript"></script>
<script src="<%=basePath%>enterprise/managejob/js/manage_job.js" type="text/javascript"></script>
</head>

<body>
	<div id="wrap">
		<s:include value="../../common/page/head_enterprise.jsp"></s:include>
		<div id="main">
			<div class="main_inner">
				<input name="" value="发布新职位" id="btn_publish" type="button"
					style="width:316px; height:44px; background:#fea201; font-size:16px; color:#fdfdfd; cursor:pointer; margin-left:705px;" />
				<br />
				<ul>
					<li><a href="javascript:void(0)" class="menu_link on_b"
						onclick="showPage('1',this)">发布中的职位</a>
					</li>
					<li><a href="javascript:void(0)" class="menu_link"
						onclick="showPage('2',this)">已结束的职位</a>
					</li>
					<li><a href="javascript:void(0)" class="menu_link"
						onclick="showPage('0',this)">未发布的职位</a>
					</li>
					<br />
				</ul>
				<div id="page_contain" class="on">
					<s:include value="joblist1.jsp"></s:include>
				</div>
				<div id="Pagination" class="right flickr" style="margin-top:10px"></div>
			</div>
		</div>
		<s:include value="../../common/page/footer.jsp"></s:include>
	</div>
</body>
</html>
