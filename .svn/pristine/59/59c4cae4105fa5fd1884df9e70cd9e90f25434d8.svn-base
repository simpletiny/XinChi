package seentao.xhsn.mail.service.impl;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import seentao.xhsn.bean.HsUserBO;
import seentao.xhsn.mail.dao.IMailConfirmDAO;
import seentao.xhsn.mail.service.IMailConfirmService;

@Service
public class MailConfirmServiceImpl implements IMailConfirmService{

	@Autowired
	private IMailConfirmDAO dao;
	
	@Override
	public HsUserBO getUserByPrimaryKey(String user_id) {
		return dao.getUserByPrimaryKey(user_id);
	}
	
}