<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
 <%@taglib uri="/struts-tags" prefix="s" %>
<%
String path = request.getContextPath();
String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
pageContext.setAttribute("basePath", basePath);
%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>新道人才网</title>
<link rel="stylesheet" type="text/css" href="<%=basePath %>common/css/basic.css"  />
<link rel="stylesheet" type="text/css" href="<%=basePath %>enterprise/manageresume/css/jlgl.css" />
<link rel="stylesheet" type="text/css" href="<%=basePath %>common/css/page.css" />
<script src="<%=basePath %>common/js/jquery-1.8.1.min.js" type="text/javascript"></script>
<script src="<%=basePath%>common/js/layer-v1.8.1/layer/layer.min.js"></script>
<script src="<%=basePath %>common/js/jquery.pagination.js" type="text/javascript"></script>
<script src="<%=basePath %>common/js/city_arr.js" type="text/javascript"></script>
<script src="<%=basePath %>common/js/funtype_arr.js" type="text/javascript"></script>
<script src="<%=basePath %>common/js/WdatePicker/WdatePicker.js" type="text/javascript"></script>
<script src="<%=basePath %>enterprise/manageresume/js/manage_resume.js" type="text/javascript"></script>
</head>

<body>
	  <div class="transparent"></div>
	<div id="tanchu" style="display:none">
		<h4>
			<a href="javascript:void(0)" class="close"></a>
		</h4>
		<div class="tanchu_inner">
			<form action="" method="post" name="myform" id="myform"> 
				<input type="hidden" name="informBO.ent_id"  value="${informBO.ent_id }"/> 
				<%--<input type="hidden" name="informBO.job_id"  value="${releaseJobBO.id }"/>--%>
				<input type="hidden" name="informBO.ent_name"  value="${informBO.ent_name}"/> 
				<%--<input type="hidden" name="informBO.job_name"  value="${releaseJobBO.rel_name }"/>--%>
				<p>公司名称：${informBO.ent_name}</p>
				<p style="color:red">*面试地址：<input type="text" name="informBO.ent_address" value="${informBO.ent_address }" style="width:400px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
				<p style="color:red">*面试岗位：<input type="text" name="informBO.job_name" value="${informBO.job_name }" style="width:400px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
				<p style="color:red">&nbsp;&nbsp;&nbsp;*联系人：<input name="informBO.interviewer" type="text" value="${informBO.interviewer}" style="width:90px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/>&nbsp;&nbsp;&nbsp; *联系电话：<input name="informBO.interviewer_phone" type="text" value="${informBO.interviewer_phone}" style="width:200px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
				<p style="color:red">*需带资料：<input name="informBO.attach" type="text" style="width:350px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);" value="纸质简历2份、毕业证等相关证件" /></p>
				<p style="color:red">*面试时间：<input name="informBO.inform_time" type="text" onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm'})" style="width:200px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
			</form>
		</div>
		<p style="line-height: 30px; margin-left: 125px;">
			<input name="" type="checkbox" value="" style="margin-right: 10px;" />注：如无法参加本次面试，请及时通知我公司，谢谢！
		</p>
		<a href="javascript:void(0)" onclick="sendinform()"
			style="display: block; width: 190px; height: 46px; line-height: 48px; text-align: center; color: #FFF; background: #fea201; font-size: 16px; border-bottom: 2px solid #c9b387; margin-left: 300px; margin-top: 10px;">立即发送</a>
		</div>
	<div id="wrap">
    	<s:include value="../../common/page/head_enterprise.jsp"></s:include>
      <div id="main">
      	<ul>
            	<li><a href="javascript:void(0)"  class="sd_b">收到的简历</a></li>
            	<li><a href="javascript:void(0)" class="gz_b">关注的简历</a></li>
                <br />
            </ul>
            <div class="sd">
            <p class="zt">简历状态：
            <input type="hidden" value="0"/>
            <input name="" value="全部"type="button" class="sx"/>
            <input type="hidden" value="N"/>
            <input name="" value="未读" type="button"/>
            <input type="hidden" value="Y"/>
            <input name="" value="已读" type="button"/>
            </p>
            <p class="yq">学历要求：
            <input type="hidden" value="0"/>
            <input name="" value="全部" type="button" class="sx"/>
            <s:iterator value="educationList" id="edu">
            	<input type="hidden" value="${edu.value}"/>
	            <input name="" value="${edu.value}" type="button"/>
            </s:iterator>
            <input type="hidden" value="-1"/>
            <input name="" value="其他" type="button"/>
            </p>
            <div id="data_container" style="height:452px">
	   			
            </div>
			<div id="Pagination" class="right flickr" style="margin-top:10px"></div>
           </div> 
      </div>
   
      <s:include value="../../common/page/footer.jsp"></s:include>
    </div>
</body>
</html>
