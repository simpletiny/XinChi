<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
 <%@taglib uri="/struts-tags" prefix="s" %>
<%
String path = request.getContextPath();
String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
%>
<s:hidden name="page.totalRecord" id="hid_sum_cnt"></s:hidden>
<table width="1024" cellspacing="0px"  style="border-collapse:collapse; border:0; color:#636262; background:#ececec;  text-align:center; margin-top:20px; margin-bottom:0px; font-weight:bold;">
	<tr>
    	<td width="195" height="45">浏览日期</td>
    	<td width="300">公司名称</td>
    	<td width="225">公司规模</td>
    	<td width="180">所属行业</td>
    	<td width="124">查看次数</td>
    </tr>
</table>
<s:iterator value="visitlist" id="vis">
<table width="1024" cellspacing="0px"  style="border-collapse:collapse; border:0; color:#636262; background:#fdfdfd;  text-align:center;border-bottom:1px solid #e5e4e4;">
	<tr>
    	<td width="195" height="45"><s:property value="#vis.visitDate"/></td>
    	<td width="300"><a href="###"><s:property value="#vis.comName"/></a></td>
    	<td width="225"><s:property value="#vis.comSize"/></td>
    	<td width="180" class="tdComType"><s:property value="#vis.comType"/></td>
    	<td width="124"><s:property value="#vis.visitCnt"/>次</td>
    </tr>
</table>
</s:iterator>