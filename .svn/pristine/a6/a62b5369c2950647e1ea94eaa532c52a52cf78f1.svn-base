package seentao.xhsn.sys.scorecatalog.dao.impl;

import java.util.List;

import org.apache.ibatis.session.SqlSession;
import org.mybatis.spring.support.SqlSessionDaoSupport;
import org.springframework.stereotype.Repository;

import seentao.xhsn.bean.HsScoreCatalogBO;
import seentao.xhsn.common.DaoUtil;
import seentao.xhsn.sys.scorecatalog.dao.IScoreCatalogDAO;


@Repository
public class ScoreCatalogDAOImpl extends SqlSessionDaoSupport implements IScoreCatalogDAO{

	private SqlSession sqlSession;
	private DaoUtil daoUtil;
	public void initDao(){
		if(daoUtil==null){
			sqlSession=sqlSession==null?getSqlSession():sqlSession;
			daoUtil=new DaoUtil(sqlSession);
		}
	}
	
	@Override
	public void insert(HsScoreCatalogBO bo) {
		daoUtil.insertBO("seentao.xhsn.bean.mappers.HsScoreCatalogBOMapper.insert", bo);
	}

	@Override
	public void update(HsScoreCatalogBO bo) {
		daoUtil.updateByPK("seentao.xhsn.bean.mappers.HsScoreCatalogBOMapper.updateByPrimaryKey", bo);
	}

	@Override
	public void delete(String id) {
		daoUtil.deleteByPK("seentao.xhsn.bean.mappers.HsScoreCatalogBOMapper.deleteByPrimaryKey", id);
	}

	@Override
	public HsScoreCatalogBO selectByPrimaryKey(String id) {
		return (HsScoreCatalogBO) daoUtil.selectByPK("seentao.xhsn.bean.mappers.HsScoreCatalogBOMapper.selectByPrimaryKey", id);
	}

	@Override
	public List<HsScoreCatalogBO> getAllByParam(HsScoreCatalogBO bo) {
		List<HsScoreCatalogBO> list=daoUtil.selectByBOParamT("seentao.xhsn.bean.mappers.HsScoreCatalogBOMapper.selectByParam", bo);
		return list;
	}
	
}