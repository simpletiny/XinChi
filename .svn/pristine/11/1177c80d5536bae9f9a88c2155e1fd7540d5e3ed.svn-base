 <select id="selectByPram" parameterType="seentao.xhsn.bean.HsLearnpathBO" resultMap="BaseResultMap">
   select 
    <include refid="Base_Column_List" />
    from hs_learnpath
   <where>
	    <if test="identity_card !=null">
	    	identity_card = #{identity_card,jdbcType=VARCHAR}
	    </if>
    </where>
   </select>
   
   <select id="selectByIdentityCard" parameterType="seentao.xhsn.bean.HsLearnpathBO"
		resultMap="NewResult">
		select DISTINCT
		sc.score_name,sc.score_code,IFNULL(l.score,'') score
		FROM hs_score_catalog sc LEFT JOIN hs_learnpath l ON sc.score_code=l.score_code
		<if test="identity_card !=null">
			AND l.identity_card = #{identity_card,jdbcType=VARCHAR}
		</if>
		ORDER BY sc.catalog_index
	</select>
	
	<select id="selectByUserID" parameterType="java.lang.String"
		resultMap="NewResult">
		select DISTINCT
		sc.score_name,sc.score_code,IFNULL(l.score,'') score
		FROM hs_score_catalog sc 
		LEFT JOIN hs_userbasicinfo b ON   b.id=#{id,jdbcType=CHAR}
		LEFT JOIN hs_learnpath l ON sc.score_code=l.score_code AND b.user_idnum=l.identity_card
		ORDER BY sc.catalog_index
	</select>
	
	<insert id="insertWithList" parameterType="java.util.List" >
    insert into hs_learnpath (id,identity_card,score_code,score,creater,create_time,modifier,modify_time)
