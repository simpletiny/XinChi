function handleResponse(data,action,param){
	$("#content").empty();
	 $.each(data.boList, function(i, item) {
		 var tr="<tr>";
            tr=tr+
                    "<td>" + item.name + "</td>" + 
                    "<td>" + item.sex    + "</td>" +
                    "<td>" + item.age + "</td>";
       	 $("#content").append(tr+"</tr>");
      });
	 appendPage(data.page,action,param);  
}