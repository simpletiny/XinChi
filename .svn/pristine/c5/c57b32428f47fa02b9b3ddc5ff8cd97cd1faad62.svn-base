@charset "utf-8";
/* CSS Document */
a {text-decoration:none; font-size:14px; cursor:pointer;}
a { blr:expression(this.onFocus=this.blur()) } /*针对 IE*/
a { outline:none; } /*针对firefox等*/
body,div,ul,li { padding: 0; margin: 0; }
img { border: none; }
*{ margin:0; padding:0; font-family:"微软雅黑",Arial; color:#636262;}
ul,ol,li{list-style:none;}
br{clear:both; font-size:0; line-height:0;}
input,textarea{ border:none; margin:0px; overflow:hidden;}
body{ background:#f5f5f5;}
#wrap{ width:100%;}
#top{ width:100%; background:url(../images/zhucetop.jpg); margin-bottom:25px;}
.top_inner{ width:1024px; height:179px; margin:0 auto; position:relative;}
#main{ width:900px; margin: auto; background:#fdfdfd; border-bottom:2px solid #e5e4e4; margin-bottom:25px;}
#main h4{ height:40px; line-height:42px; font-size:16px; color:#38ac96; background:#dcf0ee; border-bottom:2px solid #dae5e4; padding-left:30px;}

@import url(alpha.css);
#maskLayer{position: absolute;left:0px;top:0px}
#alphadiv{position:absolute;background:#000;z-index: 2000}
#drag{position:absolute;background:#fff;z-index: 2001;width:580px;border:1px solid #ccc}
#drag_h{line-height:28px;height:28px;cursor:move;color:#fff;background:#38ac96;border-bottom:1px dashed #babcbc;padding:0 10px;}
#drag h3 b{float:left; color:#FFF; font-size:14px;}
#drag h3 span{font-weight:normal;float:right;cursor:pointer; color:#FFF; font-size:13px;}

#sublist{position:absolute;z-index:2100;background:#fff}
#btn{background:url(btn.png) no-repeat;width:183px;height:21px;border:none;cursor:pointer}
#btn_residency,#btn_jobArea,
#btn_FuntypeID,#btn_FuntypeID_2,
#btn_IndustryID,#btn_IndustryID_2,
#btn_hukou,#btn_nation,
#btn_MajorID,#btn_MajorID_2{background:url(../images/select.jpg) no-repeat;width:198px;height:35px;border:none;cursor:pointer; font-size:14px;}

