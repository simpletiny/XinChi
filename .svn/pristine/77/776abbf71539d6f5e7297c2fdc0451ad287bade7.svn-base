package seentao.xhsn.sys.job.dao.impl;

import java.util.List;

import org.apache.ibatis.session.SqlSession;
import org.mybatis.spring.support.SqlSessionDaoSupport;
import org.springframework.stereotype.Service;

import seentao.xhsn.bean.HsJobBO;
import seentao.xhsn.common.DaoUtil;
import seentao.xhsn.sys.job.dao.IJobDAO;
import seentao.xhsn.tools.Page;

/** 
 * @author wjx 
 * @date 2015年1月8日 下午2:07:21 
 * @note 
 */
@Service
public class JobDAOImpl extends SqlSessionDaoSupport implements IJobDAO {


	private SqlSession sqlSession;
	private DaoUtil daoUtil;

	public void initDao() {
		if (daoUtil == null) {
			sqlSession = sqlSession == null ? getSqlSession() : sqlSession;
			daoUtil = new DaoUtil(sqlSession);
		}
	}
	
	@Override
	public List<HsJobBO> findAllJob(Page<HsJobBO> page) {
		return daoUtil.selectByParam("seentao.xhsn.bean.mappers.HsJobBOMapper.selectByPage", page);
	}

	@Override
	public HsJobBO getByPK(String id) {
		return (HsJobBO) daoUtil.selectByPK("seentao.xhsn.bean.mappers.HsJobBOMapper.selectByPrimaryKey", id);
	}

	@Override
	public int updateJobByPK(HsJobBO bo) {
		return daoUtil.updateByPK("seentao.xhsn.bean.mappers.HsJobBOMapper.updateByPrimaryKey", bo);
	}

	@Override
	public String insert(HsJobBO bo) {
		return daoUtil.insertBO("seentao.xhsn.bean.mappers.HsJobBOMapper.insert", bo);
	}

	@Override
	public List<HsJobBO> findJobsByPage(Page<HsJobBO> page) {
		return daoUtil.selectByParam("seentao.xhsn.bean.mappers.HsJobBOMapper.selectByPage", page);
	}

	@Override
	public int deleteById(String id) {
		return daoUtil.deleteByPK("seentao.xhsn.bean.mappers.HsJobBOMapper.deleteByPrimaryKey", id);
	}

	@Override
	public String getMaxCodeByParam(HsJobBO param) {
		return daoUtil.selectOneValueByParam("seentao.xhsn.bean.mappers.HsJobBOMapper.getMaxCode", param);
	}

	@Override
	public List<HsJobBO> findAllJobsByParam(HsJobBO param) {
		return daoUtil.selectByBOParamT("seentao.xhsn.bean.mappers.HsJobBOMapper.findAllByParam", param);
	}
	

}
