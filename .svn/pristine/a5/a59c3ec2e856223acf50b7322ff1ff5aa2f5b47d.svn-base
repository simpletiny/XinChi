<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<!-- 简历预览模板 -->
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>简历预览</title>
<link rel="stylesheet" type="text/css" href="${basePath}common/css/basic.css" />
<link rel="stylesheet" type="text/css" href="${basePath}common/css/jlyl.css" />
<script src="${basePath}common/js/jquery-1.8.1.min.js" type="text/javascript"></script>
<script src="${basePath}common/js/layer-v1.8.1/layer/layer.min.js"></script>
<script src="${basePath}person/resume/js/resume.js"></script>
<script type="text/javascript" src="${basePath}common/js/drag.js"></script>
<script type="text/javascript" src="${basePath}common/js/WdatePicker/WdatePicker.js"></script>
<script type="text/javascript" src="${basePath}common/js/major_arr.js"></script>
<script src="${basePath}common/js/city_arr.js" type="text/javascript"></script>
<script src="${basePath}common/js/funtype_arr.js" type="text/javascript"></script>
<script src="${basePath}common/js/industry_arr.js" type="text/javascript"></script>
</head>
<body>
	<style>
	#main{ width:21cm; margin:0 auto; padding-bottom:1.5cm;padding-top:1.5cm; position:relative;}
	#main h2{ width:21cm; text-align:center; font-size:20px; line-height:42px;}
	#main div{ width:19cm; background:#FFF; border-bottom:1px solid #e5e4e4; margin-bottom:10px; padding-bottom:10px; margin-left:1cm;}
	#main div h4{ width:19cm; height:50px; position:relative;}
	#main h4 a{ font-size:10px; color:#636262; position:absolute; left:976px; top:17px; font-weight:normal;}
	#main div p{ margin-left:40px; line-height:1.8em;font-size:12px;margin-right:40px}
	#main div p span{ margin-left:15px;}
	</style>
       <div id="main">
      		<h2>${res.name}</h2>
      		
			<div>
				<h4 style="background:url(${basePath}common/images/resume/images/xxlj2.png);"></h4>
				<table cellspacing="0px"
					style="width:17cm;border-collapse:collapse; border:0;  text-align:center; margin-left:50px;">
			
					<tr>
						<s:iterator value="pathlist" id="path" >
						<td style="width:3cm" height="45">${path.scoreCatalogBO.score_name}</td>
						</s:iterator>
					</tr>
					<tr>
						<s:iterator value="pathlist" id="path" >
						<td style="width:3cm" height="45" style="color:#059e81;">${path.score}</td>
						</s:iterator>
					</tr>
				</table>
			</div>
		</div>
		
<!--endprint1-->
</body>
</html>

 