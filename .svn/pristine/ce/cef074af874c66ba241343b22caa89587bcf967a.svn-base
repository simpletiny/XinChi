#main{ width:586px; height:568px; margin:0 auto;}
#main ul li{ position:relative; float:left;}
#main .company,#main .person{ display:block; float:left; width:293px; height:99px; line-height:100px; font-size:24px; color:#13a085; background:#dcf0ee; border-bottom:2px solid #e6e6e6; text-align:center; position:relative;}
#main .active{ background:#FFF; border:none;}
#main ul li div{ position:absolute; width:511px; height:355px; background:#FFF; padding:53px 0 0 75px; border-bottom:2px solid #e5e4e4;}
#main ul li div ul li { float:none;}
#main ul li .person_inner{ left:0px; top:101px;} 
#main ul li div .sr,#main ul li div .srmm{ width:427px; height:46px; line-height:46px; font-size:16px; color:#adacac; background:url(../images/sr.png) no-repeat; padding-left:15px;}
#main ul li div .sr,#main ul li div .srmm1{ width:427px; height:46px; line-height:46px; font-size:16px; color:#adacac; background:url(../images/sr.png) no-repeat; padding-left:15px;}
#main ul li div li a{ font-size:14px; color:#a5a5a5;}
#main ul li div li .wjmm{ margin-left:310px;}
#main ul li div li .wjmm:hover{ color:#38ac96;}
#main ul li div li .dl{ width:432px; height:46px; background:#38ac96; cursor:pointer; margin-top:30px;font-size:20px;color:#FFF}
#main ul li .company_inner{ left:-293px; top:101px;} 
