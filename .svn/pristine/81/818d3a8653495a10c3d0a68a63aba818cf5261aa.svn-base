@charset "utf-8";
/* CSS Document */
#main{ width:100%; background:#e5ebe9; padding-top:20px;}
@import url(alpha.css);
#main .tj{ width:1024px; margin:0 auto;}
#main .tj table td .cur{ cursor:pointer;display:block; line-height:1.8em; padding-left:5px;}
#main .tj table td .cur:hover{ color:#FFF; background:#38ac96;}
.zx{ width:1024px; margin:0 auto; background:#f5f5f5; padding:15px 0;}
.zx table{ margin-bottom:5px;}
#maskLayer{position: absolute;left:0px;top:0px}
#alphadiv{position:absolute;background:#000;z-index: 2000}
#drag{position:absolute;background:#fff;z-index: 2001;width:580px;border:1px solid #ccc}
#drag_h{line-height:28px;height:28px;cursor:move;color:#fff;background:#38ac96;border-bottom:1px dashed #babcbc;padding:0 10px;}
#drag h3 b{float:left; color:#FFF;}
#drag h3 span{font-weight:normal;float:right;cursor:pointer; color:#FFF;}

#sublist{position:absolute;z-index:2100;background:#fff}
#btn{background:url(btn.png) no-repeat;width:183px;height:21px;border:none;cursor:pointer}
#btn_residency,#btn_jobArea,
#btn_FuntypeID,#btn_FuntypeID_2,
#btn_IndustryID,#btn_IndustryID_2,
#btn_hukou,#btn_nation,
#btn_MajorID,#btn_MajorID_2{background:url(../images/select.jpg) no-repeat;width:198px; padding-left:5px;height:35px;border:none;cursor:pointer; font-size:14px; text-align:left;}
/* 选择居住地 */
#residencyAlpha{padding:10px 4px}
#residencyAlpha h4{color:#454545;font-size:14px; font-weight:bold;}
#residencyAlpha dl{zoom:1;overflow:hidden;line-height:25px}
#residencyAlpha dt{float:left;width:80px;font-weight:bold}
#residencyAlpha dd{float:left;width:492px}
#residencyAlpha dd li{float:left;width:82px;cursor:pointer;text-indent:4px;height:25px;overflow:Hidden}
#residencyAlpha dd li.over{background:#38ac96;color:#fff}
#allProv{border-top:1px dashed #ccc;margin-top:6px;padding-top:10px}
/* 居住地 悬浮子菜单 */
#sub_city{border:1px solid #38ac96;line-height:25px;text-align:left}
#sub_city h4{border-bottom:1px dashed #ccc}
#sub_city ul{overflow:hidden;zoom:1}
#sub_city ul li{float:left;width:60px;height:25px;}
#sub_city ul a{display:block;height:25px;color:#000;text-decoration:none;text-indent:4px}
#sub_city ul a:hover{background:#38ac96;color:#fff}

#sublist{position:absolute;z-index:2100;background:#fff}
/* 选择行业列表 */
#IndustryAlpha{padding:10px 4px}
#IndustryAlpha dl{zoom:1;overflow:hidden;line-height:25px;border-bottom:1px dashed #ccc}
#IndustryAlpha dt{float:left;width:200px;font-weight:bold;font-size:14px;color:#38ac96;}
#IndustryAlpha dd{float:left;width:450px}

#IndustryAlpha ul{zoom:1;overflow:Hidden;line-height:25px;padding:8px 0}
#IndustryAlpha li{background-image:url(../images/chk_off.gif);float:left;width:220px;cursor:pointer;text-indent:20px;background-repeat:no-repeat;height:25px;overflow:Hidden;background-position:6px 6px}
#IndustryAlpha li.over{background-color:#38ac96;color:#fff}
#IndustryAlpha li.chkON{background-image:url(../images/chk_on.gif)}
#IndustryAlpha .radio li{background-image:none}
/* 选择职能类别列表 */
#FuntypeAlpha{padding:10px 4px}
#FuntypeAlpha dl{zoom:1;overflow:hidden;line-height:25px;border-bottom:1px dashed #ccc}
#FuntypeAlpha dt{float:left;width:200px;font-weight:bold;font-size:14px;color:#38ac96}
#FuntypeAlpha dd{float:left;width:450px}

#FuntypeAlpha ul{zoom:1;overflow:Hidden;line-height:25px;padding:8px 0}
#FuntypeAlpha li{float:left;width:220px;cursor:pointer;text-indent:20px;height:25px;overflow:Hidden;background-repeat:no-repeat;background-position:6px 6px}
#FuntypeAlpha li.over{background-color:#38ac96;color:#fff}
#FuntypeAlpha li.chkON{background-image:url(../images/chk_on.gif)}
/* 职能类别 悬浮子菜单 */
#sub_funtype{border:1px solid #38ac96;line-height:25px;text-align:left}
#sub_funtype h4{border-bottom:1px dashed #ccc}
#sub_funtype ul{overflow:hidden;zoom:1}
#sub_funtype ul li{float:left;width:220px;height:25px;overflow:hidden}
#sub_funtype ul a{display:block;height:25px;color:#000;text-decoration:none;text-indent:16px;background-image:url(../images/chk_off.gif);background-repeat:no-repeat;background-position:2px 6px}
#sub_funtype ul a:hover{background-color:#38ac96;color:#fff}
#sub_funtype ul a.chkON{background-image:url(../images/chk_on.gif)}
#sublist .radio a{background-image:none}

.transparent {
	left: 0;
	top: 0;
	position: absolute;
	filter: alpha(opacity = 50);
	-moz-opacity: 0.5;
	-khtml-opacity: 0.5;
	opacity: 0.5;
	background: #000;
	z-index: 10;
}

#tanchu {
	position: fixed;
	width: 832px;
	height: 494px;
	background: url(../images/tanchubg.png) no-repeat;
	z-index: 1000;
}

#tanchu h4 {
	height: 58px;
	position: relative;
}

#tanchu h4 a {
	display: block;
	width: 32px;
	height: 32px;
	background: url(../images/close.png) no-repeat;
	position: absolute;
	left: 762px;
	top: 14px;
}

.tanchu_inner {
	width: 582px;
	background: #FFF;
	margin-left: 125px;
	margin-top: 30px;
}

.tanchu_inner p {
	line-height: 50px;
	margin-left: 50px;
}
