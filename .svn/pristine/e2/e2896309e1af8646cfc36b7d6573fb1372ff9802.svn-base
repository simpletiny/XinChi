<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
 <%@taglib uri="/struts-tags" prefix="s" %>
<%
String path = request.getContextPath();
String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
%>
<s:hidden name="page.totalRecord" id="hid_sum_cnt"></s:hidden>
<ul>
<s:iterator value="recommendedJobList" id="job">
	<li>
		<s:hidden name="job_id" cssClass="jobId"></s:hidden>
    	<h4><input name="chkJob" type="checkbox" value=""  style="margin-right:10px;"/><s:property value="#job.jobName"/><span><s:property value="#job.publishTime"/></span></h4>
            <h5><s:property value="#job.company_name"/><a href="###">收藏职位</a></h5>
            <p>地点：<s:property value="#job.county_code"/>&nbsp;&nbsp;公司性质：<s:property value="#job.comProperty"/>&nbsp;&nbsp;公司规模：<s:property value="#job.comSize"/>  经验：<s:property value="#job.jobExp"/>&nbsp;&nbsp;学历：<s:property value="#job.jobEducation"/>&nbsp;&nbsp;职位月薪：<s:property value="#job.jobSalary"/></p>
            <p>岗位职责：<s:property value="#job.jobDuty"/>[更多]</p>
     
<br />

<input onclick="applyJob(this)" type="button" value="立即申请" style=" width:88px; height:28px; background:#fea201; margin-left:55px; margin-top:10px; cursor:pointer; color:#fdfdfd;"/>
    </li>
</s:iterator>
</ul>
<p style="line-height:46px; border-bottom:1px solid #e5e4e4; background:#ececec; padding-left:55px;"><input name="" type="checkbox" value="" id="chk_all" onclick="checkAll()" style="margin-right:10px;"/>全选<input name="" type="button" onclick="applyJobs()" value="批量申请" style=" width:88px; height:28px; background:#fea201; margin-left:50px; cursor:pointer; color:#fdfdfd;"/><input name="" type="button" value="批量收藏" style=" width:88px; height:28px; background:#cfd2d7; margin-left:40px; cursor:pointer; color:#636262;"/></p>
