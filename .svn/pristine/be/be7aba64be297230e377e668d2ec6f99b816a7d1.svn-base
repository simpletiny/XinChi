@charset "utf-8";
/* CSS Document */
#main{ width:1024px; margin:0 auto; padding:30px 0;}
.main_inner ul{ width:1024px; border-bottom:1px solid #d0d1d1; height:55px;}
.main_inner ul li { float:left;}
.main_inner ul li a{ display:block; width:200px; height:55px; line-height:55px; font-size:16px; color:#565555; text-align:center;}
.main_inner ul li .active{border-bottom:1px solid #08b897; color:#08b897;}
.main_inner table{ margin-top:5px;}
.main_inner table td span{ margin-right:20px;}
.main_inner .on table{ background:#FFF;}
.main_inner .close table{ background:#efefef;}
