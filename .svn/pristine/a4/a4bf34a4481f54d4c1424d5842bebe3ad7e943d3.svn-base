/* 选择行业列表 */
#IndustryAlpha {
	padding: 10px 4px
}

#IndustryAlpha dl {
	zoom: 1;
	overflow: hidden;
	line-height: 25px;
	border-bottom: 1px dashed #ccc
}

#IndustryAlpha dt {
	float: left;
	width: 200px;
	font-weight: bold;
	font-size: 14px;
	color: #f90
}

#IndustryAlpha dd {
	float: left;
	width: 450px
}

#IndustryAlpha ul {
	zoom: 1;
	overflow: Hidden;
	line-height: 25px;
	padding: 8px 0
}

#IndustryAlpha li {
	float: left;
	width: 220px;
	cursor: pointer;
	text-indent: 20px;
	height: 25px;
	overflow: Hidden;
	background-position: 6px 6px
}

#IndustryAlpha li.over {
	background-color: #38ac96;
	color: #fff
}

#IndustryAlpha li.chkON {
	background-image: url(chk_on.gif)
}

#IndustryAlpha .radio li {
	background-image: none
}
