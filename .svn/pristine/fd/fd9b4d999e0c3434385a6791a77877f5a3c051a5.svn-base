#main .nav, #main .info {
	float: left;
}

#main .nav {
	margin-right: 25px;
	border-bottom: 2px solid #e5e4e4;
}

#main .nav .big_pic {
	width: 218px;
	height: 115px;
	background: #FFF;
	border-bottom: 2px solid #e5e4e4;
	margin-bottom: 20px;
}

#main .nav .big_pic h4 {
	height: 43px;
	background: url(../images/leftmenu/myjl.jpg) no-repeat;
}

#main .nav .big_pic p {
	font-size: 16px;
	color: #636262;
	margin-left: 18px;
	margin-top: 10px;
	margin-bottom: 10px;
}

#main .nav .big_pic  a {
	color: #636262;
	margin-right: 18px;
}

#main .nav .nav_inner li {
	background: #FFF;
	position: relative;
}

#main .nav .nav_inner li .prev {
	position: absolute;
	left: 174px;
	top: 14px;
}

#main .nav .nav_inner {
	width: 218px;
	overflow: hidden;
}

#main .nav .nav_inner li a {
	display: block;
	width: 163px;
	height: 40px;
	padding-left: 55px;
	line-height: 40px;
	font-size: 16px;
	color: #636262;
	background: url(../images/leftmenu/totally.png) 19px center no-repeat;
}

#main .nav .nav_inner li:hover {
	background: #ffcc72;
	font-weight: bold;
}

#main .nav .nav_inner .bjs {
	background: #ffcc72;
	font-weight: bold;
}


