.transparent_res {
	left:0;
	top:0;
	position:absolute;
	  filter:alpha(opacity=50);  
	  -moz-opacity:0.5;  
	  -khtml-opacity: 0.5;  
	  opacity: 0.5;
	  background:#000;
	  z-index:10;
	  display:none;
} 
