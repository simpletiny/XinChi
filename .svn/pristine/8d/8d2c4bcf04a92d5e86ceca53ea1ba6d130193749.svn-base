<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@taglib uri="/struts-tags" prefix="s"%>

<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<base href="<%=basePath%>" />
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>新道人才网</title>
<link rel="stylesheet" type="text/css"
	href="person/jobSearch/css/job_search.css" />
<link rel="stylesheet" type="text/css" href="common/css/basic.css" />
<link rel="stylesheet" type="text/css" href="common/css/page.css" />
<script src="common/js/jquery-1.8.1.min.js" type="text/javascript"></script>
<script type="text/javascript" src="person/jobSearch/js/job_search.js"></script>
<script type="text/javascript" src="common/js/drag.js"></script>
<script type="text/javascript" src="common/js/funtype_arr.js"></script>
<script type="text/javascript" src="common/js/funtype_func.js"></script>
<script type="text/javascript" src="common/js/industry_arr.js"></script>
<script type="text/javascript" src="common/js/industry_func.js"></script>
<script type="text/javascript" src="common/js/city_arr.js"></script>
<script type="text/javascript" src="common/js/city_func.js"></script>
<script type="text/javascript" src="common/js/page.js" ></script>

</head>

<body>
	<div id="wrap">
		<jsp:include page="/common/page/head_person.jsp" />
		<div id="search">
			<div class="search_inner">
				<form action="" name="myform" id="myform" method="post">
				<ul>
					<li style="margin-left: 0px;">
						<p>职位类别</p> 
						<s:set name="jobText" value="请选择"></s:set>
			            <s:if test='job_text != null && job_text != ""'>
			            	<s:set name="jobText" value="job_text"></s:set>
			            </s:if>
						<input id="btn_FuntypeID" type="button" value="${jobText }" onclick="funtypeSelect_2()" />
						<input id="FuntypeID" type="hidden" name="bo.job_id" value="${bo.job_id }"/>
					</li>
					<li>
						<p>行业类别</p> 
						<s:set name="tradeText" value="请选择"></s:set>
			            <s:if test='trade_text != null && trade_text != ""'>
			            	<s:set name="tradeText" value="trade_text"></s:set>
			            </s:if>
						<input id="btn_IndustryID" type="button" value="${tradeText }" onclick="IndustrySelect()" />
						<input id="IndustryID" type="hidden" name="bo.trade_id" value="${bo.trade_id }" />
					</li>
					<li>
						<p>工作地点</p> 
						<s:set name="jobAreaText" value="请选择"></s:set>
			            <s:if test='city_text != null && city_text != ""'>
			            	<s:set name="jobAreaText" value="city_text"></s:set>
			            </s:if>
						<input id="btn_jobArea" type="button" value="${jobAreaText }" onclick="jobAreaSelect()" />
						<input id="jobAreaID" type="hidden" value="${bo.county_code }"  name="bo.county_code" />
					</li>
					<br />
				</ul>
				<s:set name="bo_rel_name" value="'请输入您的邮箱（新道学员帐号）'"></s:set>
	            <s:if test='bo.keyword != null && bo.keyword != ""'>
	            	<s:set name="bo_rel_name" value="bo.keyword"></s:set>
	            </s:if>
				 <input name="bo.keyword" value="${bo_rel_name }" onfocus="if(value=='请输入公司名称或职位名称') {value=''}" onblur="if (value=='') {value='请输入公司名称或职位名称'}" type="text" style="width:403px; height:44px; line-height:44px; float:left; font-size:14px; color:#636262; padding-left:5px;"/>
				<input name="btn_job_search" onclick="formsub()" type="button" style="width: 316px; height: 44px; background: url(person/jobSearch/images/bt_sj.jpg) no-repeat; cursor:pointer; float:left; margin-left:30px;" />
				<!-- <a class="gj">高级搜索</a> -->
				<br/>
				</form>
			</div>
		</div>
		<div id="main">
      	<div class="main_inner">
        	<p>多于100000个职位满足条件</p>
            <ul class="result">
            <li style="font-weight:bold; margin-bottom:0px;">
                <ul>
                    <li class="job_n" style="background:#ececec;">职位名称</li>
                	<li class="company_n" style="background:#ececec;">公司名称</li>
                	<li class="money" style="background:#ececec;">职位月薪</li>
                	<li class="place" style="background:#ececec;">工作地点</li>
                	<li class="date" style="background:#ececec;">发布日期</li>
                    <br />
				</ul>
            </li>
            <div id="content">
            	<s:iterator value="jobs" id="bean">
            	<li>
		 			<ul>
			            <li class="job_n">
			            	<a style="color:#05a687;" 
				            href="person/jobDetailInit?releaseJobId=${bean.id}">
				            <input name="job" type="checkbox" value="${bean.id}" />${bean.jobName}</a>
			            </li>
			        	<li class="company_n"><a style="color:#05a687;" 
			        	href="person/jobDetailInit?releaseJobId=${bean.id}">${bean.companyName}</a></li>
			        	<li class="money">${bean.monthlyPay}</li>
			        	<li class="place" name="jobAddress">${bean.jobAddress}</li>
			        	<li class="date">${bean.publishDate}</li>
			            <br />
			      	</ul> 
		       	 </li>
            	</s:iterator>
            </div>
            <div id="page">

    		</div>
            </ul>
            <form action="" method="post" name="putform" id="putform">
            	<input type="hidden" id="res_id" name="res_id" value="" />
            	<input type="hidden" id="job_ids" name="job_ids" value="" />
            </form>
            <table width="1024" cellspacing="0px"  style="border-collapse:collapse; border:0; color:#636262; background:#ececec; margin-top:-5px;">
            <tr>
            	<td width="250" height="40"><input id="btn_job"  onclick="jobSelectAll()" type="checkbox" value=""  style="margin:0 10px; vertical-align:central;"/>全选</td>
            	<td width="300"><input name="" onclick="putresume()" value="申请职位" type="button" style="width:88px; height:28px; background:#fea201; vertical-align:central; color:#FFF; cursor:pointer;" />
            	<input onclick='plsczw()' value="收藏职位"type="button" style="width:88px; height:28px; background:#cfd2d7; vertical-align:central; margin-left:30px; cursor:pointer;" /></td>
            	<td width="185"></td>
            	<td width="165"></td>
            	<td width="124"></td>
            </tr>
            </table>
        </div>
        <br  />
      </div>
      <jsp:include page="/common/page/footer.jsp" />
    </div>
    <!-- alpha div -->
<div id="maskLayer" style="display:none">
<iframe id="maskLayer_iframe" frameBorder=0 scrolling=no style="filter:alpha(opacity=50)"></iframe>
<div id="alphadiv" style="filter:alpha(opacity=50);-moz-opacity:0.5;opacity:0.5"></div>
	<div id="drag">
		<h3 id="drag_h"></h3>
		<div id="drag_con"></div><!-- drag_con end -->
	</div>
</div><!-- maskLayer end -->
</div>
<!-- alpha div end -->
<div id="sublist" style="display:none"></div>
</body>
</html>