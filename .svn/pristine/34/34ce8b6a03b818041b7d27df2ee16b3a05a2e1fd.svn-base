package seentao.xhsn.bean;

import java.io.Serializable;
import seentao.xhsn.common.SupperBO;

public class HsGroupMailRecordBO extends SupperBO implements Serializable {
	private static final long serialVersionUID = 1L;
		
		 
		  private java.lang.String receiver;
		  
		 
		  private java.lang.String title;
		  
		 
		  private java.lang.String content;
		  
		 
		  private java.sql.Timestamp send_time;
		  
		 
		  public  java.lang.String getReceiver() {
				return receiver;
			}
			
			public void setReceiver( java.lang.String receiver) {
				this.receiver = receiver;
			}
		 
		  public  java.lang.String getTitle() {
				return title;
			}
			
			public void setTitle( java.lang.String title) {
				this.title = title;
			}
		 
		  public  java.lang.String getContent() {
				return content;
			}
			
			public void setContent( java.lang.String content) {
				this.content = content;
			}
		 
		  public  java.sql.Timestamp getSend_time() {
				return send_time;
			}
			
			public void setSend_time( java.sql.Timestamp send_time) {
				this.send_time = send_time;
			}
	

}
