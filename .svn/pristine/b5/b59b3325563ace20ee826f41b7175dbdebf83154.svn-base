package seentao.xhsn.person.subjoin.dao.impl;

import org.apache.ibatis.session.SqlSession;
import org.mybatis.spring.support.SqlSessionDaoSupport;
import org.springframework.stereotype.Service;

import seentao.xhsn.bean.HsSubjoinBO;
import seentao.xhsn.common.DaoUtil;
import seentao.xhsn.person.subjoin.dao.ISubjoinDAO;

/** 
 * @author wjx 
 * @date 2014年12月12日 上午11:02:07 
 * @note 
 */
@Service
public class SubjoinDAOImpl extends SqlSessionDaoSupport implements ISubjoinDAO {

	private SqlSession sqlSession;
	private DaoUtil daoUtil;

	public void initDao() {
		if (daoUtil == null) {
			sqlSession = sqlSession == null ? getSqlSession() : sqlSession;
			daoUtil = new DaoUtil(sqlSession);
		}
	}
	
	@Override
	public int update(HsSubjoinBO subjoinBO) {
		
		return daoUtil.updateByPK("seentao.xhsn.bean.mappers.HsSubjoinBOMapper.updateByPrimaryKey", subjoinBO);
	}

	@Override
	public String insert(HsSubjoinBO subjoinBO) {
		return daoUtil.insertBO("seentao.xhsn.bean.mappers.HsSubjoinBOMapper.insert", subjoinBO);
	}

	@Override
	public HsSubjoinBO getByParam(HsSubjoinBO param) {
		return daoUtil.selectOneValueByParam("seentao.xhsn.bean.mappers.HsSubjoinBOMapper.selectByPram", param);
	}

}
