<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
 <%@taglib uri="/struts-tags" prefix="s" %>
 <s:hidden name="page.totalRecord" id="hid_sum_cnt"></s:hidden>
   <s:iterator value="jobList" id="job">
     	<table width="1024" cellspacing="0px"  style="border-collapse:collapse; border:0; font-size:14px; color:#656464;border-bottom:1px solid #d7d6d6;">
         <tr>
         	<td width="540" height="40" style="font-size:16px; padding-left:50px;"><s:property value="#job.jobName"/></td>
         	<td width="424" style="text-align:center;"><a href="javascript:void(0)" onclick="getReceivedResume(this)" style="color:#05a687; font-size:16px;"><s:property value="#job.resumeCnt"/>份简历应聘</a></td>
       		<td style="display:none" class="tdJobId"><input type="hidden" value="${job.rjboId}"/></td>
		 </tr>
         <tr>
         	<td width="540" height="40" style="font-size:16px; padding-left:50px;"><span><s:property value="#job.jobExp"/></span><span><s:property value="#job.education"/></span>
           </td>
         	<td width="424" style="font-size:16px; color:#05a687; text-align:center;"><span><a href="javascript:void(0)" onclick="refreshJob('${job.rjboId}',this)">刷新</a></span><span><a href="javascript:modifyJob('${job.rjboId}')">修改</a></span><span><a href="javascript:endJob('${job.rjboId }')">结束</a></span><span style="margin-right:0px;"><a href="javascript:deleteJob('${job.rjboId}')">删除</a></span></td>
         </tr>
         </table>
     </s:iterator>