select, option {
	font-size: 14px;
	color: #636262;
}

table tr td {
	font-size: 14px;
	color: #636262;
}

.top_inner .logo, .top_inner .person {
	float: left;
}

.top_inner .logo {
	width: 704px;
	height: 83px;
}

.top_inner .logo img, .top_inner .logo ul {
	float: left;
}

.top_inner .logo ul {
	width: 400px;
	margin-left: 25px;
}

.top_inner .logo ul li {
	float: left;
	margin-left: 20px;
}

.top_inner .logo ul li a {
	font-size: 18px;
	color: #FFF;
	display: block;
	line-height: 83px;
}

.top_inner .logo ul li .bold {
	font-weight: bold;
	background: url(../images/triangle.jpg) no-repeat bottom center;
}

.top_inner .person {
	width: 320px;
	height: 83px;
	line-height: 83px;
	position: relative;
}

.top_inner .person .pic {
	float: left;
	width: 42px;
	height: 42px;
	border-radius: 60px;
	background: url(../images/kobe.jpg);
	margin-top: 22px;
	margin-left: 80px;
}

.top_inner .person .name {
	float: left;
	width: 180px;
	margin-left: 15px;
	color: #FFF;
	white-space: nowrap;
	text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	position: relative;
}

.top_inner .person .name:hover ul {
	display: block;
}

.top_inner .person .name ul {
	position: absolute;
	display: none;
}

.top_inner .person .name ul li a {
	display: block;
	width: 120px;
	padding-left: 30px;
	height: 26px;
	line-height: 26px;
	background: #38ac96;
	color: #FFF;
}

.top_inner .person .name ul li a:hover {
	background: #a1d8ce;
}

.top_inner .person .active {
	background: url(../images/triangle_d.png) no-repeat right center;
}

.top_inner .person .xl {
	position: absolute;
	left: 210px;
	top: 83px;
}

.top_inner .person .xl li a {
	display: block;
	width: 78px;
	padding-left: 30px;
	height: 26px;
	line-height: 26px;
	font-size: 12px;
	color: #fdfdfd;
	background: #38ac96;
}

.top_inner .person .xl li a:hover {
	background: #a1d8ce;
}



#main {
	width: 1024px;
	margin: 0 auto;
	margin-bottom: 16px;
}



#main .info {
	width: 780px;
	overflow: hidden;
}

#main .info h4 {
	width: 780px;
	height: 40px;
	position: relative;
	margin-bottom: 5px;
}

#main .info .gr {
	background: url(../images/h4bg_s.png) no-repeat left center;
}

#main .info .tj {
	background: url(../images/tj.png) no-repeat left center;
}

#main .info h4 a {
	position: absolute;
	left: 720px;
	top: 7px;
	display: block;
	width: 59px;
	height: 28px;
	background: url(../images/more.jpg);
}

#main .info .info_inner {
	width: 740px;
	height: 628px;
	background: #fdfdfd;
	padding-top: 25px;
	padding-left: 40px;
	border-bottom: 2px solid #e5e4e4;
	font-size: 16px;
	margin-bottom: 22px;
}

#main .info ul {
	width: 780px;
	min-height: 20px;
	border-top: 3px solid #fdfdfd;
}

#main .info ul li {
	width: 240px;
	float: left;
	background: #FFF;
	margin-left: 30px;
}

#main .info ul li h4 {
	font-size: 16px;
	color: #000000;
	font-weight: normal;
	line-height: 40px;
	border-bottom: 1px solid #f7f7f7;
	width: 225px;
	padding-left: 15px;
	white-space: nowrap;
	text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	overflow: hidden;
}

#main .info ul li:hover h4 {
	color: #f09901;
}

#main .info ul li p {
	width: 240px;
	height: 46px;
}

#main .info ul li p span {
	margin-left: 10px;
	font-size: 12px;
	line-height: 1.8em;
}

#main .info .info_inner table td .yj {
	color: #1ea88e;
}

#main .info .info_inner table td .wei {
	color: #e70808;
}

@import url(alpha.css);

#maskLayer {
	position: absolute;
	left: 0px;
	top: 0px
}

#alphadiv {
	position: absolute;
	background: #000;
	z-index: 2000
}

#drag {
	position: absolute;
	background: #fff;
	z-index: 2001;
	width: 580px;
	border: 1px solid #ccc
}

#drag_h {
	line-height: 28px;
	height: 28px;
	cursor: move;
	color: #fff;
	background: #38ac96;
	border-bottom: 1px dashed #babcbc;
	padding: 0 10px;
}

#drag h3 b {
	float: left;
	color: #FFF;
}

#drag h3 span {
	font-weight: normal;
	float: right;
	cursor: pointer;
	color: #FFF;
}

#sublist {
	position: absolute;
	z-index: 2100;
	background: #fff
}

#btn {
	background: url(btn.png) no-repeat;
	width: 183px;
	height: 21px;
	border: none;
	cursor: pointer
}

#btn_residency, #btn_jobArea, #btn_FuntypeID, #btn_FuntypeID_2,
	#btn_IndustryID, #btn_IndustryID_2, #btn_hukou, #btn_nation,
	#btn_MajorID, #btn_MajorID_2 {
	background: url(../images/select.jpg) no-repeat;
	width: 198px;
	height: 35px;
	border: none;
	cursor: pointer;
	font-size: 14px;
}

/* 选择居住地 */
#residencyAlpha {
	padding: 10px 4px
}

#residencyAlpha h4 {
	color: #454545;
	font-size: 14px;
	font-weight: bold;
}

#residencyAlpha dl {
	zoom: 1;
	overflow: hidden;
	line-height: 25px
}

#residencyAlpha dt {
	float: left;
	width: 80px;
	font-weight: bold
}

#residencyAlpha dd {
	float: left;
	width: 492px
}

#residencyAlpha dd li {
	float: left;
	width: 82px;
	cursor: pointer;
	text-indent: 4px;
	height: 25px;
	overflow: Hidden
}

#residencyAlpha dd li.over {
	background: #38ac96;
	color: #fff
}

#allProv {
	border-top: 1px dashed #ccc;
	margin-top: 6px;
	padding-top: 10px
}
/* 居住地 悬浮子菜单 */
#sub_city {
	border: 1px solid #38ac96;
	line-height: 25px;
	text-align: left
}

#sub_city h4 {
	border-bottom: 1px dashed #ccc
}

#sub_city ul {
	overflow: hidden;
	zoom: 1
}

#sub_city ul li {
	float: left;
	width: 60px;
	height: 25px;
}

#sub_city ul a {
	display: block;
	height: 25px;
	color: #000;
	text-decoration: none;
	text-indent: 4px
}

#sub_city ul a:hover {
	background: #38ac96;
	color: #fff
}

#sublist {
	position: absolute;
	z-index: 2100;
	background: #fff
}
#upload { position: absolute;z-index: 100; margin-left: -100px; font-size: 15px; opacity: 0;cursor:pointer;  margin-top: 137px;  filter: alpha(opacity = 0);}