select, option {
	font-size: 14px;
	color: #636262;
}

table tr td {
	font-size: 14px;
	color: #636262;
}



#att {
	width: 1024px;
	margin: 0 auto;
	background: #FFF;
	border-bottom: 2px solid #e5e4e4;
	padding: 17px 0px;
	margin-bottom: 16px;
}

#att p, #att ul {
	float: left;
}

#att p {
	width: 620px;
	height: 60px;
	padding: 20px 0 0 30px;
	line-height: 1.8em;
	font-size: 14px;
	color: #636262;
}

#att ul {
	width: 322px;
	padding-left: 50px;
	height: 80px;
	border-left: 1px solid #c5c3c3;
	color: #636262;
}

#att ul li {
	color: #636262;
	margin-bottom: 10px;
}

#att ul li .percent_box {
	width: 240px;
	background: #e5e5e5;
}

#att ul li .percent {
	width: 270px;
	height: 17px;
	background: url(../images/percent.jpg) no-repeat;
}

#att ul li:nth-child(3) {
	color: #e60012;
}

#main {
	width: 1024px;
	margin: 0 auto;
	margin-bottom: 16px;
}

#main .nav, #main .info {
	float: left;
}

#main .nav {
	margin-right: 25px;
	border-bottom: 2px solid #e5e4e4;
}

#main .nav .big_pic {
	width: 218px;
	height: 115px;
	background: #FFF;
	border-bottom: 2px solid #e5e4e4;
	margin-bottom: 16px;
}

#main .nav .big_pic h4 {
	height: 43px;
	background: url(../images/myjl.jpg) no-repeat;
}

#main .nav .big_pic p {
	font-size: 16px;
	color: #636262;
	margin-left: 18px;
	margin-top: 10px;
	margin-bottom: 10px;
}

#main .nav .big_pic  a {
	color: #636262;
	margin-right: 18px;
}

#main .nav .nav_inner li {
	background: #FFF;
	position: relative;
}

#main .nav .nav_inner li .prev {
	position: absolute;
	left: 174px;
	top: 14px;
}

#main .nav .nav_inner {
	width: 218px;
	overflow: hidden;
}

#main .nav .nav_inner li a {
	display: block;
	width: 163px;
	height: 40px;
	padding-left: 55px;
	line-height: 40px;
	font-size: 16px;
	color: #636262;
	background: url(../images/totally.png) 19px center no-repeat;
}

#main .nav .nav_inner li:hover {
	background: #ffcc72;
	font-weight: bold;
}

#main .nav .nav_inner .bjs {
	background: #ffcc72;
	font-weight: bold;
}

#main .info {
	width: 780px;
}

#main .info h4 {
	width: 780px;
	height: 40px;
	position: relative;
	margin-bottom: 5px;
}

#main .info .gr {
	background: url(../images/qzyx.png) no-repeat left center;
}

#main .info .tj {
	background: url(../images/tj.png) no-repeat left center;
}

#main .info h4 a {
	position: absolute;
	left: 720px;
	top: 7px;
	display: block;
	width: 59px;
	height: 28px;
	background: url(../images/change.jpg);
}

#main .info .info_inner {
	width: 740px;
	background: #fdfdfd;
	padding-top: 25px;
	padding-left: 40px;
	border-bottom: 2px solid #e5e4e4;
	font-size: 16px;
}

#main .info ul {
	width: 780px;
	min-height: 20px;
	border-top: 3px solid #fdfdfd;
}

#main .info  table td ul li {
	line-height: 1.8em;
	padding-left: 15px;
}

#main .info  table td ul li:hover {
	background: #38ac96;
}

#main .info  table td ul li:hover a {
	color: #FFF;
}

#main .info  table td ul li a {
	color: #636262;
}

#main .info .info_inner1 {
	width: 780px;
	font-size: 14px;
}

#main .info .info_inner1 .jl_inner {
	width: 720px;
	padding: 15px 0 15px 60px;
	background: #fdfdfd;
	border-bottom: 2px solid #e5e4e4;
	margin-bottom: 10px;
}

#main .info .info_inner1 .jl_inner table td {
	font-size: 14px;
}

@import url(alpha.css);

#maskLayer {
	position: absolute;
	left: 0px;
	top: 0px
}

#alphadiv {
	position: absolute;
	background: #000;
	z-index: 2000
}

#drag {
	position: absolute;
	background: #fff;
	z-index: 2001;
	width: 580px;
	border: 1px solid #ccc
}

#drag_h {
	line-height: 28px;
	height: 28px;
	cursor: move;
	color: #fff;
	background: #38ac96;
	border-bottom: 1px dashed #babcbc;
	padding: 0 10px;
}

#drag h3 b {
	float: left;
	color: #FFF;
}

#drag h3 span {
	font-weight: normal;
	float: right;
	cursor: pointer;
	color: #FFF;
}

#sublist {
	position: absolute;
	z-index: 2100;
	background: #fff
}

#btn {
	background: url(btn.png) no-repeat;
	width: 183px;
	height: 21px;
	border: none;
	cursor: pointer
}

#btn_residency, #btn_jobArea, #btn_FuntypeID, #btn_FuntypeID_2,
	#btn_IndustryID, #btn_IndustryID_2, #btn_hukou, #btn_nation,
	#btn_MajorID, #btn_MajorID_2 {
	background: url(../images/select.jpg) no-repeat;
	width: 198px;
	height: 35px;
	border: none;
	cursor: pointer;
	font-size: 14px;
}

