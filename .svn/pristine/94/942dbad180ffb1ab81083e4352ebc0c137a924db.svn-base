/* 职能类别 悬浮子菜单 */
#sub_funtype {
	border: 1px solid #38ac96;
	line-height: 25px;
	text-align: left
}

#sub_funtype h4 {
	border-bottom: 1px dashed #ccc
}

#sub_funtype ul {
	overflow: hidden;
	zoom: 1
}

#sub_funtype ul li {
	float: left;
	width: 220px;
	height: 25px;
	overflow: hidden
}

#sub_funtype ul a {
	display: block;
	height: 25px;
	color: #000;
	text-decoration: none;
	text-indent: 16px;
	background-image: url(../images/chk_off.gif);
	background-repeat: no-repeat;
	background-position: 2px 6px
