package seentao.xhsn.sys.filedata.service.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

import seentao.xhsn.bean.HsFileDataBO;
import seentao.xhsn.sys.filedata.dao.IFileDataDAO;
import seentao.xhsn.sys.filedata.service.IFileDataService;

@Service
@Transactional
public class FileDataServiceImpl implements IFileDataService{

	@Autowired
	private IFileDataDAO dao;
	
	@Override
	public void insert(HsFileDataBO bo) {
		dao.insert(bo);
	}

	@Override
	public void update(HsFileDataBO bo) {
		dao.update(bo);
	}

	@Override
	public void delete(String id) {
		dao.delete(id);
	}

	@Override
	public HsFileDataBO selectByPrimaryKey(String id) {
		return dao.selectByPrimaryKey(id);
	}

	@Override
	public List<HsFileDataBO> getAllByParam(HsFileDataBO bo) {
		return dao.getAllByParam(bo);
	}
	
}