<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ page import="seentao.xhsn.common.DictionaryEnum" %>

<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@taglib uri="/struts-tags" prefix="s" %> 

<%
String path = request.getContextPath();
String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";

String user_type = DictionaryEnum.ENTERPRISE_TYPE.getStringValue();
%>
    
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<base href="<%=basePath %>"/>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>新道人才网-企业注册</title>
<link rel="stylesheet" type="text/css" href="share/register/css/regist_ent.css"  />
<script src="common/js/jquery-1.8.1.min.js" type="text/javascript"></script>
<script type="text/javascript" src="common/js/city_func.js"></script>
<script type="text/javascript" src="common/js/city_arr.js"></script>
<script type="text/javascript" src="common/js/drag.js"></script>
<script type="text/javascript" src="share/register/js/regist_ent.js" ></script>
<script type="text/javascript">

</script>
</head>

<body>
<div id="wrap">
    	<jsp:include page="/common/page/head_ent.jsp" />
        <div id="main">
        	<h4>公司信息</h4>
        	<form action="" method="post" name="myform" id="myform">
        		<s:token></s:token>
        		<s:fielderror cssStyle="color:red"></s:fielderror>
        		<span style="color:red">${fail_msg }</span>
        		<input type="hidden" id="user_type" name="bo.user_type" value="<%=user_type%>"/>
	            <table width="700" cellspacing="0px"  style="border-collapse:collapse; border:0; font-size:16px; color:#626060; margin:15px 0 20px 145px;">
		           	<tr>
		            	<td width="120" style="padding-left:20px; " height="20"></td>
		            	<td >&nbsp;<span style="color:red;font-size:12px;" id="ent_name_err"></span></td>
		            </tr>
		            <tr>
		            	<s:set name="ent_name_text" value="'企业全称'"></s:set>
	                    <s:if test='entBO.ent_name != null && entBO.ent_name != ""'>
	                    	<s:set name="ent_name_text" value="entBO.ent_name"></s:set>
	                    </s:if>
		            	<td width="120" style="padding-left:20px; background:url(share/register/images/red.png) left center no-repeat;" height="50">企业名称</td>
		            	<td ><input id="ent_name" name="entBO.ent_name"  value="${ent_name_text }" onfocus="if(value=='企业全称') {value=''}" onblur="validateEntName();if (value=='') {value='企业全称'}" type="text" style="width:425px; padding-left:7px; height:46px; line-height:46px; background:url(share/register/images/input.png); font-size:15px; color:#626060;"/></td>
		            </tr>
		            <tr>
		            	<td width="120" style="padding-left:20px; " height="20"></td>
		            	<td >&nbsp;<span style="color:red;font-size:12px;" id="county_code_err"></span></td>
		            </tr>
		            <tr>
		            	<s:set name="county_code_value_text" value="'请选择'"></s:set>
	                    <s:if test='entBO.county_code_value != null && entBO.county_code_value != ""'>
	                    	<s:set name="county_code_value_text" value="entBO.county_code_value"></s:set>
	                    </s:if>
		            	<s:set name="county_code_text" value="''"></s:set>
	                    <s:if test='entBO.county_code != null && entBO.county_code != ""'>
	                    	<s:set name="county_code_text" value="entBO.county_code"></s:set>
	                    </s:if>
		            	<td width="120" style="padding-left:20px; background:url(share/register/images/red.png) left center no-repeat;" height="50">所在地</td>
		            	<td >
							<input id="btn_jobArea" type="text" readonly="readonly" name="entBO.county_code_value" value="${county_code_value_text }" onclick="jobAreaSelect()"  style="width:425px; padding-left:7px; height:46px; line-height:46px; background:url(share/register/images/input.png); font-size:15px; color:#626060;""/>
			        		<input id="jobAreaID" type="hidden" name="entBO.county_code" value="${county_code_text }"  />
						</td>
		            </tr>
		            <tr>
		            	<td width="120" style="padding-left:20px; " height="20"></td>
		            	<td >&nbsp;<span style="color:red;font-size:12px;" id="com_people_err"></span></td>
		            </tr>
		            <tr>
		            	<s:set name="com_people_text" value="'联系人姓名'"></s:set>
		            	<s:if test='entBO.com_people != null && entBO.com_people != ""'>
	                    	<s:set name="com_people_text" value="entBO.com_people"></s:set>
	                    </s:if>
		            	<td width="120" style="padding-left:20px; background:url(share/register/images/red.png) left center no-repeat;" height="50">联系人</td>
		            	<td  style="font-size:15px;"><input id="com_people" name="entBO.com_people" value="${com_people_text }" onfocus="if(value=='联系人姓名') {value=''}" onblur="validateEntpeople();if (value=='') {value='联系人姓名'}" type="text" style="width:271px; padding-left:7px; height:46px; line-height:46px; background:url(share/register/images/lxr.png); font-size:15px; color:#626060;"/>
		                <input id="com_people_sex" name="entBO.com_people_sex" type="radio"  style="margin:0 10px;" value="0"/>先生
		                <input id="com_people_sex" name="entBO.com_people_sex" type="radio"  style="margin:0 10px;" value="1"/>女士
		                </td>
		            </tr>
		            <tr>
		            	<td width="120" style="padding-left:20px; " height="20"></td>
		            	<td >&nbsp;<span style="color:red;font-size:12px;" id="com_telephone_err"></span></td>
		            </tr>
		            <tr>
		            	<s:set name="com_telephone_area_code_text" value="'区号'"></s:set>
	                    <s:if test='entBO.com_telephone_area_code != null && entBO.com_telephone_area_code != ""'>
	                    	<s:set name="com_telephone_area_code_text" value="entBO.com_telephone_area_code"></s:set>
	                    </s:if>
		            	<s:set name="com_telephone_text" value="'固定电话'"></s:set>
	                    <s:if test='entBO.com_telephone != null && entBO.com_telephone != ""'>
	                    	<s:set name="com_telephone_text" value="entBO.com_telephone"></s:set>
	                    </s:if>
	                    <s:set name="com_telephone_extension_text" value="'分机号'"></s:set>
	                    <s:if test='entBO.com_telephone_extension != null && entBO.com_telephone_extension != ""'>
	                    	<s:set name="com_telephone_extension_text" value="entBO.com_telephone"></s:set>
	                    </s:if>
		            	<td width="120" style="padding-left:20px; background:url(share/register/images/red.png) left center no-repeat;" height="50">联系电话</td>
		            	<td >
		                <input id="area_code" name="entBO.com_telephone_area_code" value="${com_telephone_area_code_text }" onfocus="if(value=='区号') {value=''}" onblur="validatetelephone_area();if (value=='') {value='区号'}" type="text" style="width:76px; padding-left:7px;  height:46px; line-height:46px; background:url(share/register/images/qh.png); font-size:15px; color:#626060;"/>
		                <input id="com_telephone" name="entBO.com_telephone" value="${com_telephone_text }" onfocus="if(value=='固定电话') {value=''}" onblur="validatetelephone_text();if (value=='') {value='固定电话'}" type="text" style="width:189px; padding-left:7px;  height:46px; line-height:46px; background:url(share/register/images/gddh.png); font-size:15px; color:#626060; margin:0 23px;"/>
		                <input id="com_telephone_extension" name="entBO.com_telephone_extension" value="${com_telephone_extension_text }" onfocus="if(value=='分机号') {value=''}" onblur="validatetelephone();if (value=='') {value='分机号'}" type="text" style="width:89px; padding-left:7px;  height:46px; line-height:46px; background:url(share/register/images/fjh.png); font-size:15px; color:#626060;"/><br />
		                </td>
		            </tr>
		            <tr>
		            	<td width="120" style="padding-left:20px; " height="20"></td>
		            	<td ><span style="color:red;font-size:12px;" id="com_phone_err"></span></td>
		            </tr>
		            <tr>
		            	<s:set name="com_phone_text" value="'手机号码'"></s:set>
	                    <s:if test='entBO.com_phone != null && entBO.com_phone != ""'>
	                    	<s:set name="com_phone_text" value="entBO.com_telephone"></s:set>
	                    </s:if>
		            	<td width="140" height="50"></td>
		            	<td ><input id="com_phone" name="entBO.com_phone"  value="${com_phone_text }" onfocus="if(value=='手机号码') {value=''}" onblur="validatePhoneNum();if (value=='') {value='手机号码'}" type="text"  onKeyPress="validateNum()" style="width:425px; padding-left:7px; height:46px; line-height:46px; background:url(share/register/images/input.png); font-size:15px; color:#626060;"/></td>
		            </tr>
		            <tr>
		            	<td width="140" height="30"></td>
		            	<td  style="font-size:14px; color:#a5a5a5;"><input name="" type="checkbox" value="" style="margin-right:10px; vertical-align:middle;" />勾选此项，会有招聘顾问优先联系您</td>
		            </tr>
		      	</table>
		    	<h4>帐户信息</h4>
		    	<table width="700" cellspacing="0px"  style="border-collapse:collapse; border:0; font-size:16px; color:#626060; margin:15px 0 20px 145px;">
		            <tr>
		            	<td width="120" style="padding-left:20px; " height="20"></td>
		            	<td >&nbsp;<span style="color:red;font-size:12px;" id="user_email_err"></span><span style="color:green;font-size:12px;" id="user_email_success"></span></td>
		            </tr>
		            <tr>
		            	<s:set name="user_email_text" value="'请填写您的注册邮箱'"></s:set>
	                    <s:if test='bo.user_email != null && bo.user_email != ""'>
	                    	<s:set name="user_email_text" value="bo.user_email"></s:set>
	                    </s:if>
		            	<td width="120" style="padding-left:20px; background:url(share/register/images/red.png) left center no-repeat;" height="50">电子邮箱</td>
		            	<td >
		            		<input name="bo.user_email" id="user_email"  value="${user_email_text }" 
		            		onfocus="if(value=='请填写您的注册邮箱') {value=''}"  
		            		onblur="validatemail();if (value=='') {value='请填写您的注册邮箱'}" 
		            		type="text" style="width:425px; padding-left:7px; height:46px; line-height:46px; 
		            		background:url(share/register/images/input.png); font-size:15px; color:#626060;"/> <input type="button" value="确认邮箱" onclick="checkEmail()"/>
		            	</td>
		            </tr>
		            <%-- <tr>
		            	<td width="120" style="padding-left:20px; " height="20"></td>
		            	<td >&nbsp;<span style="color:red;font-size:12px;" id="user_name_err"></span></td>
		            </tr>
		            <tr>
		            	<td width="120" style="padding-left:20px; background:url(share/register/images/red.png) left center no-repeat;" height="50">用户名</td>
		            	<td ><input id="user_name" name="bo.user_name" value="请填写您的用户名" onfocus="if(value=='请填写您的用户名') {value=''}" onblur="validatename();if (value=='') {value='请填写您的用户名'}" type="text" style="width:425px; padding-left:7px; height:46px; line-height:46px; background:url(share/register/images/input.png); font-size:15px; color:#626060;"/></td>
		            </tr> --%>
		            <tr>
		            	<td width="120" style="padding-left:20px; " height="20"></td>
		            	<td>&nbsp;<span style="color:red;font-size:12px;" id="user_pwd_err"></span></td>
		            </tr>
		            <tr>
		            	<td width="120" style="padding-left:20px; background:url(share/register/images/red.png) left center no-repeat;" height="50">密码</td>
		            	<td  style="font-size:15px; position:relative;">
		                 	<span style="position:absolute; display:block; width:280px; left:5px; top:18px; font-size:15px; color:#626060; " class="mima2">请填写您的注册密码（至少6位）</span>
		                	<input id="user_pwd" name="bo.user_pwd" type="password" 
		                	style="width:271px; padding-left:7px; height:46px; line-height:46px; 
		                	background:url(share/register/images/lxr.png); font-size:15px; color:#626060; 
		                	position:absolute; z-index:100;left:0px; bottom:0px;" onblur="validatepswd()"
		                	class="srmm2"/>
		               	</td>
		            </tr>
		            <tr>
		            	<td width="120" style="padding-left:20px; " height="20"></td>
		            	<td >&nbsp;<span style="color:red;font-size:12px;" id="user_pwd2_err"></span></td>
		            </tr>
		            <tr>
		            	<td width="120" style="padding-left:20px; background:url(share/register/images/red.png) left center no-repeat;" height="50">确认密码</td>
		            	<td  style="font-size:15px; position:relative;">
		                 	<span style="position:absolute; display:block; width:280px; left:5px; top:18px; font-size:15px; color:#626060; " class="qrmm">请确认您的注册密码（至少6位）</span>
		                	<input id="user_pwd2" name="bo.user_pwd2" type="password" style="width:271px; padding-left:7px; height:46px; line-height:46px; 
		                	background:url(share/register/images/lxr.png); font-size:15px; color:#626060; 
		                	position:absolute; z-index:100;left:0px; bottom:0px;" onblur="validatepswd2()"  class="qr"/>
		                </td>
		            </tr>
		            <tr>
		            	<td width="140" height="30"></td>
		            	<td style="font-size:14px; color:#a5a5a5;"><input id="mobileAcceptIpt" onclick="agreelic()" type="checkbox" value="" style="margin-right:10px; vertical-align:middle;" /><a href="share/register/protocol.html" target="_blank">我已阅读并同意新道人才《服务条款》</a><span style="color:red;font-size:12px;" id="mobileAcceptIpt_err"></span></td>
		            </tr>
		            <tr>
		            	<td width="140" height="80"></td>
		            	<td style="font-size:14px; color:#a5a5a5;"><input id="reg_btn" disabled  onclick="formsub()" value="免 费 注 册" type="button" style="width:432px; height:46px; background:#999999; cursor:pointer; font-size:20px; color:#FFF;"/></td>
		            </tr>
		        </table>
            </form>
        </div>
       
    	<jsp:include page="/common/page/footer.jsp" />
    </div>
    <!-- alpha div -->
<div id="maskLayer" style="display:none">
<iframe id="maskLayer_iframe" frameBorder=0 scrolling=no style="filter:alpha(opacity=50)"></iframe>
<div id="alphadiv" style="filter:alpha(opacity=50);-moz-opacity:0.5;opacity:0.5"></div>
	<div id="drag">
		<h3 id="drag_h"></h3>
		<div id="drag_con"></div><!-- drag_con end -->
	</div>
</div><!-- maskLayer end -->
</div>
<!-- alpha div end -->
<div id="sublist" style="display:none"></div>
</body>
</html>
