
function upload() {
	var layIndex=layer.load('上传中,请稍候...', -1);
	$("#myForm").ajaxSubmit(
			{
				type : 'post',
				url : "sys/uploadEnterprise",
				dataType : "text",
				success : function(data) {
					layer.close(layIndex);
					if(data.length>0)
						$("#errorTip").text(data);
					else
						alert("导入失败，请重新上传！");
				},
				error : function(XmlHttpRequest, textStatus, errorThrown) {
					layer.close(layIndex);
					alert("导入失败，请重新上传！");
				}
			});
}