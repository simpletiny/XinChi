<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>


<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@taglib uri="/struts-tags" prefix="s"%>

<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<base href="<%=basePath%>" />
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>新道人才网</title>
<link rel="stylesheet" type="text/css" href="<%=basePath%>person/info/css/personal_edit.css" />
<link rel="stylesheet" type="text/css" href="<%=basePath%>common/css/basic.css" />
<script src="<%=basePath%>common/js/jquery-1.8.1.min.js" type="text/javascript"></script>
<script src="<%=basePath%>common/js/jquery.form.min.js" type="text/javascript"></script>
<script type="text/javascript" src="<%=basePath%>common/js/WdatePicker/WdatePicker.js"></script><%-- 
<script type="text/javascript" src="person/info/js/jquery.js"></script> --%>
<script type="text/javascript" src="<%=basePath%>person/info/js/drag.js"></script>
<script type="text/javascript" src="<%=basePath%>person/info/js/city_arr.js"></script>
<script type="text/javascript" src="<%=basePath%>person/info/js/city_func.js"></script>
<script type="text/javascript" src="<%=basePath%>person/info/js/info.js"></script>
<script src="common/js/layer-v1.8.1/layer/layer.min.js"></script>

<script type="text/javascript">
	window['basePath'] = "<%=basePath%>";
	// 是否在数组内
	function in_array(needle, haystack) {
		if (typeof needle == 'string' || typeof needle == 'number') {
			for ( var i in haystack) {
				if (haystack[i] == needle) {
					return true;
				}
			}
		}
		return false;
	}
	$(document).ready(function(){
		switchTab(3);
		var hukou_v = "${subjoinBO.aear_code}";
		var residency_v = "${subjoinBO.ub_address}";
		if(hukou_v.length > 0){
			$("#btn_hukou").val(ja[hukou_v]);
		}
		if(residency_v.length > 0){
			$("#btn_residency").val(ja[residency_v]);
		}
	})
</script>
</head>

<body>
	<div id="wrap">
		<jsp:include page="/common/page/head_person.jsp" />
		<jsp:include page="/common/page/hint_person.jsp" />

		<div id="main">
			<jsp:include page="/common/page/leftmenu_person.jsp" />
			<div class="info">
				<h4 class="gr"></h4>
				<div class="info_inner">
					<form method="post" name="myform" id="myform" enctype="multipart/form-data">
					<span id="err_msg" style="font-weight:bold; color:red;">${err_msg }</span>
					<input type="hidden" name="lm_tab_name" value="${lm_tab_name }" />
					<input type="hidden" name="bo.id" value="${bo.id }" />
					<input type="hidden" name="subjoinBO.id" value="${subjoinBO.id }" />
					<input type="hidden" name="user_id" value="${user_id }" />
					<input type="hidden" name="res_id" value="${res_id }" />
					<input type="hidden" name="bo.user_passport" value="${bo.user_passport }" />
					<table width="740" cellspacing="0px"
						style="border-collapse: collapse; border: 0; color: #6d6e71;">
						<tr>
							<td width="145" height="48" style="padding-left: 30px;">姓名：</td>
							<td width="275" style="text-align: left;"><input type="hidden" name="bo.user_name" value="${bo.user_name }" />${bo.user_name }</td>
							<td width="320" rowspan="4"><img
								src="${bo.user_photo }"
								style="margin-bottom: 5px; width:124px;height:149px;" id="user_logo" />
								<input type="hidden" id="txt_user_photo" name="bo.user_photo" value="${bo.user_photo }"/>
								<a  href="javascript:void(0)">修改</a><input id="upload" name="file" type="file" onchange="uploadUserLogo()" size="1"> <br /> 
						</tr>
						<tr>
							<td width="145" height="48"
								style="background: url(person/info/images/red.png) no-repeat left center; padding-left: 30px;">性别：</td>
							<td width="275" style="text-align: left;">
								<s:if test="dicts != null && dicts.sys_sex != null && dicts.sys_sex.size() > 0">
									<s:iterator value="dicts.sys_sex" id="data">
										<input id="user_gender" name="bo.user_gender" type="radio" value="${data.dictionary_code }"
										<c:if test="${bo.user_gender == data.dictionary_code }" >  
										checked  
										</c:if> 
										 style="width: 10px; height: 10px; margin-right: 10px; margin-left: 0px;" />${data.value} &nbsp;
									</s:iterator>
								</s:if>
								
								<!-- <input name="" type="radio" value="" style="width: 10px; height: 10px; margin-right: 10px; margin-left: 0px;" />男
								<input name="" type="radio" value=""style="width: 10px; height: 10px; margin-left: 10px; margin-right: 10px;" />女 -->
							
							</td>
						</tr>
						<tr>
							<td width="145" height="48"
								style="background: url(person/info/images/red.png) no-repeat left center; padding-left: 30px;">出生日期：</td>
							<td width="275" style="text-align: left;">
								<input readonly name="subjoinBO.ub_birthday" type="text" id="ub_birthday" value="${subjoinBO.ub_birthday}" onClick="WdatePicker({dateFmt:'yyyy-MM-dd'})" style="width: 142px; height: 33px; 
								line-height: 33px; background: url(common/images/big_input.jpg) repeat-x; 
								margin-right: 10px; text-align: center; font-size: 14px;"/> 
								<!-- <input name="" type="text" style="width: 82px; height: 33px; 
								line-height: 33px; background: url(person/info/images/nian.jpg) no-repeat; 
								margin-right: 10px; text-align: center; font-size: 14px;" />年
								<input name="" type="text" style="width: 55px; height: 33px; 
								line-height: 33px; background: url(person/info/images/nian.jpg) no-repeat; 
								margin: 0 10px; text-align: center; font-size: 14px;" />月 -->
								
							</td>
						</tr>
						<tr>
							<td width="145" height="48"
								style="background: url(person/info/images/red.png) no-repeat left center; padding-left: 30px;">工作经验：</td>
							<td width="275" style="text-align: left; position: relative;">
								<select id="job_exp_year" name="subjoinBO.job_exp_year" style="width: 198px;">
									<option  ></option>
									
									<s:if test="dicts != null && dicts.user_jobexp != null && dicts.user_jobexp.size() > 0">
										<s:iterator value="dicts.user_jobexp" id="data">
											<option value="${data.dictionary_code }"
											<c:if test="${subjoinBO.job_exp_year == data.dictionary_code }" >  
												selected
											</c:if> 
											>${data.value }</option>
										</s:iterator>
									</s:if>
								</select>
							</td>
						</tr>
						<tr>
							<td width="145" height="48"
								style="background: url(person/info/images/red.png) no-repeat left center; padding-left: 30px;">户口所在地：</td>
							<td width="275" style="text-align: left; position: relative;">
								
								<input id="btn_hukou" type="button" value="户口所在地" onclick="hukouSelect()" />
							</td>
							<td width="320"><input type="hidden" id="hukou" name="subjoinBO.aear_code" value="${subjoinBO.aear_code }"/></td>
						</tr>
						<tr>
							<td width="145" height="48" style="background: url(person/info/images/red.png) no-repeat left center; padding-left: 30px;"
							>现居住城市：</td>
							<td width="275" style="text-align: left; position: relative;">
								<input id="btn_residency" type="button" value="现居住城市" onclick="residencySelect()" />
							</td>
							<td width="320"><input type="hidden" id="residency" name="subjoinBO.ub_address" value="${subjoinBO.ub_address }" /></td>
						</tr>
						<tr>
							<td width="145" height="48" style="padding-left: 30px;">政治面貌：</td>
							<td colspan="2" style="text-align: left;">
								<s:if test="dicts != null && dicts.user_political != null && dicts.user_political.size() > 0">
									<s:iterator value="dicts.user_political" id="data">
										<input name="subjoinBO.political_id" type="radio" value="${data.dictionary_code }"
										<c:if test="${subjoinBO.political_id == data.dictionary_code }" >  
										checked  
										</c:if> 
										 style="width: 10px; height: 10px; margin-right: 10px; margin-left: 0px;" />${data.value} &nbsp;
									</s:iterator>
								</s:if>
								<!-- <input name="" type="radio" value="" style="width: 10px; height: 10px; margin-right: 10px; margin-left: 0px;" />中共党员（预备党员）
								<input name="" type="radio" value="" style="width: 10px; height: 10px; margin-left: 10px; margin-right: 10px;" />团员
								<input name="" type="radio" value="" style="width: 10px; height: 10px; margin-left: 10px; margin-right: 10px;" />群众
								<input name="" type="radio" value="" style="width: 10px; height: 10px; margin-left: 10px; margin-right: 10px;" />民主党派
								<input name="" type="radio" value="" style="width: 10px; height: 10px; margin-left: 10px; margin-right: 10px;" />无党派人士 -->
							</td>
						</tr>
						<tr>
							<td width="145" height="48" style="padding-left: 30px;">婚姻状况：</td>
							<td width="275" style="text-align: left;">
								<s:if test="dicts != null && dicts.sys_marrystate != null && dicts.sys_marrystate.size() > 0">
									<s:iterator value="dicts.sys_marrystate" id="data">
										<input name="subjoinBO.ub_marrystate" type="radio" value="${data.dictionary_code }"
										<c:if test="${subjoinBO.ub_marrystate == data.dictionary_code }" >  
										checked  
										</c:if> 
										 style="width: 10px; height: 10px; margin-right: 10px; margin-left: 0px;" />${data.value} &nbsp;
									</s:iterator>
								</s:if>
								<!-- <input name="" type="radio" value="" style="width: 10px; height: 10px; margin-right: 10px; margin-left: 0px;" />已婚
								<input 	name="" type="radio" value="" style="width: 10px; height: 10px; margin-left: 10px; margin-right: 10px;" />未婚
								<input name="" type="radio" value="" style="width: 10px; height: 10px; margin-left: 10px; margin-right: 10px;" />离异 -->
							</td>
						</tr>
						<tr>
							<td width="145" height="48" style="padding-left: 30px;">手机号：</td>
							<td colspan="2" width="275" style="text-align: left;">
							<input id="user_phone_text" type="text" name="bo.user_phone" onblur="valdatePhone()" value="${bo.user_phone }" style="width: 301px; height: 34px; line-height: 34px; background: url(person/info/images/input_long.jpg) no-repeat;  text-align: left; font-size: 14px; " />
							</td>
						</tr>
						<tr>
							<td width="145" height="48" style="padding-left: 30px;">身份证号：</td>
							<td colspan="2" style="text-align: left;">
								<%-- <select name="bo.ub_code_type" style="width: 198px;">
									<option ></option>
									<s:if test="dicts != null && dicts.user_code_type != null && dicts.user_code_type.size() > 0">
										<s:iterator value="dicts.user_code_type" id="data">
											<option value="${data.dictionary_code }"
											<c:if test="${bo.ub_code_type == data.dictionary_code }" >  
												selected
											</c:if> 
											>${data.value }</option>
										</s:iterator>
									</s:if>
									<!-- <option selected="selected">身份证</option>
									<option>学生证</option> -->
								</select> --%>
								<input name="bo.user_idnum" value="${bo.user_idnum }" type="text" style="width: 301px; height: 34px; line-height: 34px; background: url(person/info/images/input_long.jpg) no-repeat;  text-align: left; font-size: 14px; " />
							</td>
						</tr>
						<tr>
							<td width="145" height="48" style="padding-left: 30px;">邮箱：</td>
							<td colspan="2" style="text-align: left;"><input value="${bo.user_email }" id="user_email" name="bo.user_email"style="width: 301px; height: 34px; line-height: 34px; background: url(person/info/images/input_long.jpg) no-repeat;  text-align: left; font-size: 14px; " />
							</td>
						</tr>
						<tr>
							<td width="145" height="98" style="padding-left: 30px;"></td>
							<td width="275" style="text-align: left;"></td>
							<td width="320">
								<input name="" type="button" value="保 存" onclick="formsub()" style="width: 112px; height: 34px; background: #fea201; color: #FFF; cursor: pointer; font-size: 14px;" />
								<input name="" type="button" value="返 回" onclick="goback()" style="width: 112px; height: 34px; background: #ececec; color: #747171; cursor: pointer; font-size: 14px; margin-left: 20px;" />
							</td>
						</tr>
					</table>
					</form>
				</div>
				<h4 class="tj">
					<a href="###"></a>
				</h4>

			</div>
			<br />
		</div>
		<jsp:include page="/common/page/footer.jsp" />
	</div>


	<!-- alpha div -->
	<div id="maskLayer" style="display: none">
		<iframe id="maskLayer_iframe" frameBorder=0 scrolling=no
			style="filter: alpha(opacity = 50)"></iframe>
		<div id="alphadiv"
			style="filter: alpha(opacity = 50); -moz-opacity: 0.5; opacity: 0.5"></div>
		<div id="drag">
			<h3 id="drag_h"></h3>
			<div id="drag_con"></div>
			<!-- drag_con end -->
		</div>
	</div>
	<!-- maskLayer end -->
	</div>
	<!-- alpha div end -->
	<div id="sublist" style="display: none"></div>
</body>
</html>
