$(document).ready(function() {
	$('.close').hide();
	$('.close_b,.on_b').removeClass('active');
	$('.on_b').addClass('active');

	$('.on_b').click(function() {
		$('.on_b,.close_b').removeClass('active');
		$(this).addClass('active');
		$('.on,.close').hide();
		$('.on').show();
	});

	$('.close_b').click(function() {
		$('.on_b,.close_b').removeClass('active');
		$(this).addClass('active');
		$('.on,.close').hide();
		$('.close').show();
	});
	if ($("#logo_name").val() != "") {
		$("#img_logo").attr("src", $("#logo_name").val());
	}

	$('#btn_publish').click(function() {
		location.href = "releaseJobInit";
	});
});