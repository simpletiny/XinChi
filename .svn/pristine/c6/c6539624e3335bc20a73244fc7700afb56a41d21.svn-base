<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="person" extends="default" namespace="/person">
		<!-- 培训经历 -->
		<action name="findAllTraining" class="trainingAction" method="findAllTraining">
			<result name="SUCCESS">train/train.jsp</result>
		</action>
		<action name="*Training" class="trainingAction" method="{1}Training">
			<result name="SUCCESS" type="json"></result>
		</action>

		<!-- 求职意向 编辑 -->
		<action name="editdesired" class="desiredPositionAction"
			method="edit">
			<result name="SUCCESS">desired/desired_edit.jsp</result>
		</action>
		<!-- 求职意向 保存 -->
		<action name="savedesired" class="desiredPositionAction"
			method="save">
			<result name="SUCCESS">desired/desired_edit.jsp</result>
			<result name="FAIL">desired/desired_edit.jsp</result>
			<result name="input">desired/desired_edit.jsp</result>
		</action>

		<!-- 个人评价 编辑 -->
		<action name="editevaluate" class="evaluateAction" method="edit">
			<result name="SUCCESS">evaluate/evaluate_edit.jsp</result>
		</action>
		<!-- 个人评价 保存 -->
		<action name="saveevaluate" class="evaluateAction" method="save">
			<result name="SUCCESS">evaluate/evaluate_edit.jsp</result>
			<result name="FAIL">evaluate/evaluate_edit.jsp</result>
			<result name="input">evaluate/evaluate_edit.jsp</result>
		</action>

		<!-- 简历其他信息 -->
		<action name="findAllResumeOther" class="resumeOtherAction"
			method="findAllResumeOther">
			<result name="SUCCESS">resumeOther/other.jsp</result>
		</action>
		<action name="*ResumeOther" class="resumeOtherAction" method="{1}ResumeOther">
			<result name="SUCCESS" type="json"></result>
		</action>

		<!-- 个人信息 编辑 -->
		<action name="editubinfo" class="UBInfoAction" method="editInfo">
			<result name="SUCCESS">info/info_edit.jsp</result>
			<result name="SHOW" type="chain">showubinfo</result>
		</action>
		<!-- 个人信息 保存 -->
		<action name="saveubinfo" class="UBInfoAction" method="save">
			<result name="SUCCESS" type="chain">showubinfo</result>
			<result name="FAIL" >info/info_edit.jsp</result>
		</action>
		<!-- 个人信息 显示 -->
		<action name="showubinfo" class="UBInfoAction" method="show">
			<result name="SUCCESS">info/info_show.jsp</result>
			<result name="EDIT" type="chain">editubinfo</result>
		</action>
		<action name="userLogoUpload" class="UBInfoAction" method="userLogoUpload">
			<result name="SUCCESS" type="json"></result>
		</action>

		<!-- 专业技能 -->
		<action name="findAllCustomSkill" class="customSkillAction"
			method="findAllCustomSkill">
			<result name="SUCCESS">skill/skill.jsp</result>
		</action>

		<action name="*CustomSkill" class="customSkillAction" method="{1}CustomSkill">
			<result name="SUCCESS" type="json"></result>
		</action>
		<!-- 职业搜索 -->
		<action name="jobsearch" class="jobSearchAction" method="search">
			<result name="LIST" type="json"></result>
		</action>

		<!-- 工作经历 -->
		<action name="findAllUBCompany" class="UBCompanyAction" method="findAllUBCompany">
			<result name="SUCCESS">company/company.jsp</result>
		</action>

		<action name="*UBCompany" class="UBCompanyAction" method="{1}UBCompany">
			<result name="SUCCESS" type="json"></result>
		</action>

		<!-- 职位详情 -->
		<action name="jobDetailInit" class="jobSearchAction" method="jobDetailInit">
			<result name="success">jobDetail/job_detail.jsp</result>
		</action>

		<!-- 教育背景 -->
		<action name="findAllEducation" class="educationAction" method="findAllEducation">
			<result name="SUCCESS">education/education.jsp</result>
		</action>

		<action name="*Education" class="educationAction" method="{1}Education">
			<result name="SUCCESS" type="json"></result>
		</action>
		<!-- 项目经验 -->
		<action name="findAllExperience" class="experienceAction"
			method="findAllExperience">
			<result name="SUCCESS">experience/experience.jsp</result>
		</action>
		<action name="*Experience" class="experienceAction" method="{1}Experience">
			<result name="SUCCESS" type="json"></result>
		</action>
		<!-- 投放简历多条 -->
		<action name="putresumes" class="putResumeAction" method="putResumes">
			<result name="SUCCESS" type="json"></result>
		</action>
		<!-- 投放简历 单独 -->
		<action name="putresume" class="putResumeAction" method="putResume">
			<result name="SUCCESS">jobDetail/put_result.jsp</result>
		</action>
		<!-- 职业搜索根据求职意向  -->
		<action name="jobsearchByDesired" class="jobSearchAction" method="searchByDesired" >
			<result name="LIST" type="json"></result>
		</action>
		
		<!-- 个人中心 初始化 -->
		<action name="centerInit" class="personCenterAction" method="init">
			<result name="success">center/center.jsp</result>
		</action>
		<!-- 取得个人中心简历申请记录，人事来信等详细信息 -->
		<action name="fetchCenterDetail" class="personCenterAction" method="fetchDetail">
			<result name="success">center/${target}.jsp</result>
		</action>
		<!-- 申请职位 -->
		<action name="applyJob" class="personCenterAction" method="applyJob">
			<result name="success" type="str">resultStr</result>
		</action>
		<!-- 申请职位 -->
		<action name="getInform" class="personCenterAction" method="getBeanByParam">
			<result name="success" type="json"></result>
		</action>
		
		<action name="resumeReview" class="resumeAction" method="resumeReview">
			<result name="success">resume/resumeReview.jsp</result>
		</action>
		<!-- 修改简历名称 -->
		<action name="updateResume" class="resumeAction" method="updateResume">
			<result name="success" type="json"></result>
		</action>
		
		<!-- 学习路径 -->
		<action name="findAllLearnPath" class="learnPathAction" method="findAllLearnPath">
			<result name="SUCCESS">learnPath/learnPath.jsp</result>
		</action>
		<!-- 收藏职位 -->
		<action name="positionCollect" class="positionCollectAction" method="positionCollect">
			<result name="RESULTJSON" type="json"></result>
		</action>
		<action name="positionCollectByIDs" class="positionCollectAction" method="positionCollectByIDs">
			<result name="RESULTJSON" type="json"></result>
		</action>
		<!-- 证书管理 -->
		<action name="findAllCertificate" class="certificateAction" method="findAllCertificate">
			<result name="SUCCESS">certificate/certificate.jsp</result>
		</action>
		<action name="certificateUpload" class="certificateUploadAction" >
		</action>
		<action name="delCertificateFile" class="certificateUploadAction" method="delCertificateFile">
			<result name="RESULTJSON" type="json"></result>
		</action>
		<action name="*Certificate" class="certificateAction" method="{1}Certificate">
			<result name="SUCCESS" type="json"></result>
		</action>
		<!-- 个人首页  登陆后-->
		<action name="goHomePage" class="homePageAction" method="goHomePage">
			<result name="SUCCESS">../index/index_person.jsp</result>
		</action>
		<!-- 个人首页  异步加载职位信息 -->
		<action name="findJobByPage" class="homePageAction" method="findJobByPage">
			<result name="SUCCESS" type="json"></result>
		</action>
		<!-- 个人首页  异步加载职位信息 -->
		<action name="goSearchJob" class="homePageAction" method="goSearchJob">
			<result name="SUCCESS">jobSearch/job_search.jsp</result>
		</action>
		
		<action name="resumeExport" class="resumeAction" method="resumeExport">
			<result name="success" type="stream">  
                <param name="contentType">${contentType}</param>  
                <param name="inputName">inputStream</param>  
                <param name="contentDisposition">attachment;filename=${filename}</param>  
                <param name="bufferSize">1024</param>  
            </result>  
		</action>
	</package>

</struts>