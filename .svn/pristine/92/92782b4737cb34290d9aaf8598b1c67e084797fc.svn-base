@charset "utf-8";
/* CSS Document */
select,option{ font-size:16px; color:#636262;}
table tr td{ font-size:14px; color:#636262;}
.top_inner .logo,.top_inner .person{ float:left;}
.top_inner .logo { width:704px; height:83px;}
.top_inner .logo img,.top_inner .logo ul{ float:left;}
.top_inner .logo ul{ width:400px; margin-left:25px;}
.top_inner .logo ul li{ float:left; margin-left:20px;}
.top_inner .logo ul li a{ font-size:20px; color:#FFF; display:block;  line-height:83px;}
.top_inner .logo ul li .bold{ font-weight:bold; background:url(../images/triangle.jpg) no-repeat bottom center;}
.top_inner .person{ width:320px; height:83px; line-height:83px; position:relative;}
.top_inner .person .pic{ float:left; width:42px; height:42px; border-radius:60px; background:url(../images/kobe.jpg); margin-top:22px; margin-left:80px;}
.top_inner .person .name { float:left; width:180px; margin-left:15px; color:#FFF; white-space:nowrap; text-overflow:ellipsis;-o-text-overflow:ellipsis; position:relative; background:url(../images/triangle_up.png) no-repeat right center;}
.top_inner .person .name:hover ul { display:block;}
.top_inner .person .name ul{ position:absolute; display:none;}
.top_inner .person .name ul li a{ display:block; width:120px; padding-left:30px; height:26px; line-height:26px; background:#38ac96; color:#FFF;}
.top_inner .person .name ul li a:hover{ background:#a1d8ce;}
.top_inner .person .active{background:url(../images/triangle_d.png) no-repeat right center;}
.top_inner .person .xl { position:absolute; left:210px; top:83px;}
.top_inner .person .xl li a{ display:block; width:78px; padding-left:30px; height:26px; line-height:26px; font-size:12px; color:#fdfdfd; background:#38ac96;}
.top_inner .person .xl li a:hover{ background:#a1d8ce;}
#att{ width:1024px; margin:0 auto; background:#FFF; border-bottom:2px solid #e5e4e4; padding:17px 0px; margin-bottom:16px;}
#att p,#att ul { float:left;}
#att p { width:620px; height:60px; padding:20px 0 0 30px; line-height:1.8em; font-size:14px; color:#636262;}
#att ul{ width:322px; padding-left:50px; height:80px; border-left:1px solid #c5c3c3; color:#636262;}
#att ul li{color:#636262; margin-bottom:10px;}
#att ul li .percent_box{ width:240px; background:#e5e5e5;}
#att ul li .percent{ width:270px; height:17px; background:url(../images/percent.jpg) no-repeat;}
#att ul li:nth-child(3){ color:#e60012;}
#main{ width:1024px; margin:0 auto; margin-bottom:16px;}
#main .nav,#main .info{ float:left;}
#main .nav{ margin-right:25px; border-bottom:2px solid #e5e4e4;}
#main .nav .big_pic{width:218px; height:115px; background:#FFF; border-bottom:2px solid #e5e4e4; margin-bottom:16px;}
#main .nav .big_pic h4{ height:43px; background:url(../images/myjl.jpg) no-repeat;}
#main .nav .big_pic p{ font-size:16px; color:#636262; margin-left:18px; margin-top:10px; margin-bottom:10px;}
#main .nav .big_pic  a { color:#636262; margin-right:18px;}
#main .nav .nav_inner li{ background:#FFF; position:relative;}
#main .nav .nav_inner li .prev { position:absolute; left:174px; top:14px;}
#main .nav .nav_inner { width:218px; overflow:hidden;}
#main .nav .nav_inner li a{ display:block; width:163px; height:40px; padding-left:55px; line-height:40px; font-size:16px; color:#636262; background:url(../images/totally.png) 19px center no-repeat;}
#main .nav .nav_inner li:hover{background:#ffcc72; font-weight:bold;}
#main .nav .nav_inner .bjs{background:#ffcc72; font-weight:bold;}
#main .info{ width:780px;}
#main .info h4{ width:780px; height:40px; position:relative; margin-bottom:5px;}
#main .info .gr{background:url(../images/qtxx.png) no-repeat left center;}
#main .info h4 a{ position:absolute; left:720px; top:7px; display:block; width:59px; height:28px;  background:url(../images/plus.jpg);}
#main .info .info_inner{ width:780px;  font-size:16px;}
#main .info .info_inner .jl_inner{ width:780px; padding:15px 0 10px 0; background:#fdfdfd; border-bottom:2px solid #e5e4e4; margin-bottom:10px;}
#main .info .info_inner .jl_inner table td span{ margin-left:20px;}


#main .info .info_inner1{ width:740px; background:#fdfdfd; padding:25px 0; padding-left:40px; border-bottom:2px solid #e5e4e4; font-size:16px;}
#main .info .info_inner1 table input{ width:420px; padding-left:5px; height:33px; line-height:33px; background:url(../images/srlong.jpg); font-size:16px; color:#636262;}


#main .info .info_inner1 table td ul li{ line-height:1.8em; padding-left:15px;}
#main .info .info_inner1 table td ul li:hover{ background:#38ac96;}
#main .info .info_inner1 table td ul li:hover a{ color:#FFF;}
#main .info .info_inner1 table td ul li a{ color:#636262;}

#main .info .info_inner1 table td .cur{ cursor:pointer;display:block; line-height:1.8em;}
#main .info .info_inner1 table td .cur:hover{ color:#FFF; background:#38ac96;}