<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
 <%@taglib uri="/struts-tags" prefix="s" %>
<%
String path = request.getContextPath();
String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="pragma" content="no-cache">
<meta http-equiv="cache-control" content="no-cache">
<meta http-equiv="Cache-Control" CONTENT="no-store">
<meta http-equiv="expires" content="0">  
<title>新道人才网</title>
<link rel="stylesheet" type="text/css" href="<%=basePath%>common/css/basic.css"  />
<link rel="stylesheet" type="text/css" href="<%=basePath%>common/css/page.css"  />
<link rel="stylesheet" type="text/css" href="<%=basePath%>enterprise/resumesearch/css/ssjl.css" />
<script src="<%=basePath%>common/js/jquery-1.8.1.min.js" type="text/javascript"></script>
<script src="<%=basePath%>common/js/layer-v1.8.1/layer/layer.min.js"></script>
<script type="text/javascript" src="<%=basePath%>common/js/drag.js"></script>
<script type="text/javascript" src="<%=basePath%>common/js/city_arr.js"></script>
<script type="text/javascript"  src="<%=basePath%>common/js/city_func.js"></script>
<script type="text/javascript" src="<%=basePath%>common/js/industry_arr.js"></script>
<script type="text/javascript" src="<%=basePath%>common/js/industry_func.js"></script>
<script type="text/javascript" src="<%=basePath%>common/js/funtype_arr.js"></script>
<script type="text/javascript" src="<%=basePath%>common/js/funtype_func.js"></script>
<script type="text/javascript" src="<%=basePath%>enterprise/resumesearch/js/ssjl.js"></script>
<script type="text/javascript" src="<%=basePath%>common/js/WdatePicker/WdatePicker.js"></script>
<script type="text/javascript" src="<%=basePath%>common/js/jquery.pagination.js"></script>
</head>

<body>
	 <div class="transparent"></div>
	<div id="tanchu" style="display:none">
		<h4>
			<a href="javascript:void(0)" class="close"></a>
		</h4>
		<div class="tanchu_inner">
			<form action="" method="post" name="myform" id="myform"> 
				<input type="hidden" name="informBO.ent_id"  value="${informBO.ent_id }"/> 
				<%--<input type="hidden" name="informBO.job_id"  value="${releaseJobBO.id }"/>--%>
				<input type="hidden" name="informBO.ent_name"  value="${informBO.ent_name}"/> 
				<%--<input type="hidden" name="informBO.job_name"  value="${releaseJobBO.rel_name }"/>--%>
				<p>公司名称：${informBO.ent_name}</p>
				<p style="color:red">*面试地址：<input type="text" name="informBO.ent_address" value="${informBO.ent_address }" style="width:400px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
				<p style="color:red">*面试岗位：<input type="text" name="informBO.job_name" value="${informBO.job_name }" style="width:400px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
				<p style="color:red">&nbsp;&nbsp;&nbsp;*联系人：<input name="informBO.interviewer" type="text" value="${informBO.interviewer}" style="width:90px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/>&nbsp;&nbsp;&nbsp; *联系电话：<input name="informBO.interviewer_phone" type="text" value="${informBO.interviewer_phone}" style="width:200px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
				<p style="color:red">*需带资料：<input name="informBO.attach" type="text" style="width:350px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);" value="纸质简历2份、毕业证等相关证件" /></p>
				<p style="color:red">*面试时间：<input name="informBO.inform_time" type="text" onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm'})" style="width:200px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/manageresume/images/input_long.jpg);"/></p>
			</form>
		</div>
		<p style="line-height: 30px; margin-left: 125px;">
			<input name="" type="checkbox" value="" style="margin-right: 10px;" />注：如无法参加本次面试，请及时通知我公司，谢谢！
		</p>
		<a href="javascript:void(0)" onclick="sendinform()"
			style="display: block; width: 190px; height: 46px; line-height: 48px; text-align: center; color: #FFF; background: #fea201; font-size: 16px; border-bottom: 2px solid #c9b387; margin-left: 300px; margin-top: 10px;">立即发送</a>
		</div>
	<div id="wrap">
      <s:include value="../../common/page/head_enterprise.jsp"></s:include>
      <div id="main">
      	<form id="search_form">
      	<div class="tj">
        <table width="800" cellspacing="0px"  style=" margin:0 auto; border-collapse:collapse; border:0; font-size:14px; color:#656464;">
        	<tr>
            	<td width="130" height="54">更新日期：</td>
                <td width="270"><input onClick="WdatePicker({dateFmt:'yyyy-MM-dd'})" name="rsb.update_date" type="text" style="background:url(<%=basePath%>common/images/select.jpg) no-repeat;width:193px; padding-left:5px;height:35px;border:none;cursor:pointer; font-size:14px; text-align:left;" /></td>
                <td width="130" height="54">学位：</td>
                 <td width="270" style="position:relative;">
                 <input name="rsb.edu_degree" type="hidden" />
                 <input id="edu_degree" onclick="hide('#degree_selectbox')" type="text" style="background:url(<%=basePath%>common/images/select.jpg) no-repeat;width:193px; padding-left:5px;height:35px;border:none;cursor:pointer; font-size:14px; text-align:left;" id="am" class="am" />
                 <div id="degree_selectbox" style="display: none; position:absolute; left:0px; top:40px;z-index:1; width:196px; background:#FFF; " class="bm">
					<span onclick="pickValue('','#edu_degree',this)" class="cur">&nbsp;</span>
					<s:iterator value="degreeList" id="degree">
					 <span onclick="pickValue('${degree.value}','#edu_degree',this)" class="cur"><s:property value="#degree.value"/></span>
					</s:iterator>
				</div>
                 </td>
            </tr>
            <tr>
            	<td width="130" height="54">工作年限：</td>
                <td width="270" style="position:relative;">
                <input name="rsb.job_exp" type="hidden" id="hid_job_exp" value="${rsb.job_exp}"/>
                <input id="working_life" value="${rsb.working_life}"  type="text" onclick="hide('#working_selectbox')" style="background:url(<%=basePath%>common/images/select.jpg) no-repeat;width:193px; padding-left:5px;height:35px;border:none;cursor:pointer; font-size:14px; text-align:left;" id="am" class="am"/>
                <div id="working_selectbox" style="z-index:1;display: none; position:absolute; left:0px; top:40px; width:196px; background:#FFF; " class="bm">
				<span onclick="pickValue('','#working_life',this)" class="cur">&nbsp;</span>
				 <s:iterator value="jobExpList" id="exp">
				 	<span onclick="pickValue('${exp.dictionary_code}','#working_life',this)"class="cur"><s:property value="#exp.value"/></span>
				 </s:iterator>
                 </div>
                </td>
                <td width="130" height="54">户口所在地：</td>
                <td width="270">
                 <input id="hukou" type="hidden" name="rsb.present_address" value="${rsb.present_address}"/>
				 <input id="btn_hukou" type="button"  value="" onclick="hukouSelect()"/>
				</td>
            </tr>
            <tr>
            	<td width="130" height="54">期望工作地：</td>
                <td width="270">
                <input type="hidden" id="residency"  name="rsb.desired_area"/>
				<input id="btn_residency" type="button" value="" onclick="residencySelect()" /></td>
                <td width="130" height="54">目前工作状态：</td>
                 <td width="270" style="position:relative;">
                 <input name="rsb.work_satus" type="hidden" />
                 <input id="work_status" type="text" onclick="hide('#working_status')" style="background:url(<%=basePath%>common/images/select.jpg) no-repeat;width:193px; padding-left:5px;height:35px;border:none;cursor:pointer; font-size:14px; text-align:left;" id="am" class="am"/>
	              <div id="working_status" style="display: none; position:absolute; left:0px; top:40px; width:196px; background:#FFF; " class="bm">
					 <span onclick="pickValue('','#work_status',this)" class="cur">&nbsp;</span>
					 <s:iterator value="workStatusList" id="status">
					 	<span onclick="pickValue('${status.dictionary_code}','#work_status',this)"  class="cur"><s:property value="#status.value"/></span>
					 </s:iterator>
	              </div>
	             </td>
            </tr>
            <tr>
            	<td width="130" height="54">期望工作性质：</td>
                <td width="270" style="position:relative;">
                 <input name="rsb.desired_position_property" type="hidden" />
                <input id="txt_position_property" onclick="hide('#position_property')" type="text" style="background:url(<%=basePath%>common/images/select.jpg) no-repeat;width:193px; padding-left:5px;height:35px;border:none;cursor:pointer; font-size:14px; text-align:left;" />
	          	 <div id="position_property" style="display: none; position:absolute; left:0px; top:40px; width:196px; background:#FFF; " class="bm">
					 <span onclick="pickValue('','#txt_position_property',this)" class="cur">&nbsp;</span>
					 <span onclick="pickValue('1','#txt_position_property',this)"  class="cur">全职</span>
					 <span onclick="pickValue('2','#txt_position_property',this)"  class="cur">兼职</span>
					 <span onclick="pickValue('3','#txt_position_property',this)"  class="cur">实习</span>
	              </div>
	          	</td>
                <td width="130" height="54">所在行业：</td>
                 <td width="270">
                 <input id="IndustryID" type="hidden" name="rsb.trade_code" value="" />
				 <input id="btn_IndustryID" type="button" value="请选择" onclick="IndustrySelect()" />
	             </td>
            </tr>
            <tr>
            	<td width="130" height="54">岗位名称：</td>
                <td width="270"> 
                <input id="txt_job_name" name="rsb.job_name" value="${rsb.job_name}" style="width:190px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/resumesearch/images/input_long.jpg);"/>
	          </td>
	          <!-- 此处后期要加权限设定 -->
              <td width="130" height="54">姓名：</td>
              <td width="270">
              <input name="rsb.user_name" style="width:190px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath%>/enterprise/resumesearch/images/input_long.jpg);"/>
	          </td>
            </tr>
            <tr>
            	<td height="70" colspan="4" style="text-align:center;">
            	  <input name="" id="btn_search" type="button" style="width:316px; height:44px; background:url(<%=basePath%>enterprise/resumesearch/images/src.jpg); cursor:pointer; margin-left:6px;" />         
            	</td>
            </tr>
        </table>
      	
        </div>
        </form>
      </div>
      <div class="zx">
	      <table width="1024" cellspacing="0px"  style="border-collapse:collapse; border:0; color:#636262; background:#ececec;  text-align:center;  margin-bottom:0px; font-weight:bold;">
	        	<tr>
	            	<td width="145" height="45">最新简历</td>
	            	<td width="250"></td>
	            	<td width="50"></td>
	            	<td width="50"></td>
	            	<td width="105"></td>
	            	<td width="165"></td>
	            	<td width="135"></td>
	            	<td width="124"></td>
	            </tr>
	       </table>
      </div>
       <div class="zx" id="resume_container">
       </div>
      <div id="Pagination" class="right flickr" style="margin-top:10px"></div>
      <jsp:include page="../../common/page/footer.jsp" />
    </div>
     <!-- alpha div -->
<div id="maskLayer" style="display:none">
<iframe id="maskLayer_iframe" frameBorder=0 scrolling=no style="filter:alpha(opacity=50)"></iframe>
<div id="alphadiv" style="filter:alpha(opacity=50);-moz-opacity:0.5;opacity:0.5"></div>
	<div id="drag">
		<h3 id="drag_h"></h3>
		<div id="drag_con"></div><!-- drag_con end -->
	</div>
</div><!-- maskLayer end -->
</div>
<!-- alpha div end -->
<div id="sublist" style="display:none"></div>
</body>
</html>
