<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@taglib uri="/struts-tags" prefix="s" %> 
<%
String path = request.getContextPath();
String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
%>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="pragma" content="no-cache">
<meta http-equiv="cache-control" content="no-cache">
<meta http-equiv="Cache-Control" CONTENT="no-store">
<meta http-equiv="expires" content="0"> 
<title>新道人才网</title>
<link rel="stylesheet" type="text/css" href="<%=basePath %>common/css/basic.css"  />
<link rel="stylesheet" type="text/css" href="<%=basePath %>enterprise/info/css/qyzx_bj.css"  />
<script src="<%=basePath %>common/js/jquery-1.8.1.min.js" type="text/javascript"></script>
<script src="<%=basePath%>common/js/layer-v1.8.1/layer/layer.min.js"></script>
<script type="text/javascript" src="<%=basePath %>common/js/drag.js"></script>
<script src="<%=basePath %>common/js/jquery.form.min.js" type="text/javascript"></script>
<script src="<%=basePath %>common/js/ajaxfileupload.js" type="text/javascript"></script>
<script type="text/javascript" src="<%=basePath %>common/js/industry_arr.js"></script>
<script src="<%=basePath %>enterprise/info/js/edit.js" type="text/javascript"></script>
<script>
	window['basePath'] = "<%=basePath%>";
	function in_array(needle, haystack) {
		if(typeof needle == 'string' || typeof needle == 'number') {
			for(var i in haystack) {
				if(haystack[i] == needle) {
						return true;
				}
			}
		}
		return false;
	}
</script>
</head>

<body>
	<div id="wrap">
    	<s:include value="../../common/page/head_enterprise.jsp"></s:include>
      <form id="form_edit"  method="post" enctype="multipart/form-data">
      <s:hidden name="bo.id" id="txt_id"/>
      
      <div id="main">
        <h4></h4>
        <div class="main_inner">
        	<table width="964" cellspacing="0px"  style="border-collapse:collapse; border:0; color:#636262;">
            	<tr>
                	<td width="178" rowspan="2" style="font-size:11px; color:#a8a7a7;">
                    	<div style=" width:178px; height:162px;  position:relative;" class="hov">
                    		<span id="uploadImg" > <input type="file" name="file" style="cursor:pointer; " id="upload"  size="1" onchange="logoupload()"> 
                    	 <a style="cursor:pointer; display:block; width:98px; height:28px; background:url(<%=basePath %>enterprise/info/images/tjtx.png);"></a> </span>
	                        <img id="img_logo" style=" width:178px; height:162px;" src="<%=basePath %>enterprise/info/images/moren.jpg"/>
	                        <input type="hidden" id="logo_name" name="bo.logo_path" value="${bo.logo_path}"/>
                        </div>
                        （大小请不要超过500K）<div style="display:none">
                        	<input id="hidBtn" type="button" onclick="upload2();" value="上传"/></div>
                    </td>
                	<td width="100" height="91" style="padding-left:44px;color: red">*公司名称：</td>
                	<td width="642"><input name="bo.ent_name" id="txt_ent_name" maxlength="50" value="${bo.ent_name}" type="text" style="width:500px; padding-left:5px; height:33px; line-height:33px; background:url(<%=basePath %>enterprise/info/images/input_long.jpg);" /></td>
                </tr><%--
                <tr>
                	<td width="100" height="91" style="vertical-align:top; padding-top:11px; padding-left:44px;">简要说明：</td>
                	<td width="642"><textarea name="bo.com_brief" cols="" rows="" style="width:495px; height:62px; padding:5px; background:url(<%=basePath %>enterprise/info/images/textarea.jpg); overflow:hidden;">${bo.com_brief}</textarea></td>
                </tr>
            --%></table>
            <table width="964" cellspacing="0px"  style="border-collapse:collapse; border:0; color:#636262; margin-top:15px;">
         	 	<tr>
                	<td width="100" height="46" style="color: red">*所属行业：</td>
                	<td width="864" style="position:relative;">
                		<script type="text/javascript" src="<%=basePath %>common/js/industry_func.js"></script>
						<input id="btn_IndustryID_2" type="button" value="请选择" onclick="IndustrySelect_2()" style="background:url(<%=basePath %>common/images/select.jpg) no-repeat;width:195px; padding-left:3px;height:35px; line-height:35px; border:none;cursor:pointer; font-size:14px; text-align:left;"/>
                   		<input type="hidden" id="IndustryID_2" name="bo.trade_code" value="${bo.trade_code}"/>    
					</div>
                    </td>
                </tr>
            	<tr>
                	<td width="100" height="46" style="color: red">*公司规模：</td>
                	<td width="864" style="position:relative;">
                	<input onclick="hide('#HMF-1')" value="${bo.com_size}" type="text" style="background:url(<%=basePath %>common/images/select.jpg) no-repeat;width:195px; padding-left:3px;height:35px; line-height:35px; border:none;cursor:pointer; font-size:14px;" id="am" class="am"/>
                    <s:hidden name="bo.size_id" id="size_id"></s:hidden>
                    <div id="HMF-1" style="display: none; position:absolute; left:0px; top:40px; width:196px; background:#FFF; " class="bm">
					    <span onclick="pickSize('0',this)" class="cur">请选择</span>
						 <s:iterator value="comsizelist" id="cs" >
						  <span onclick="pickSize('${cs.dictionary_code}',this)" class="cur"><s:property value="#cs.value"/></span>
						 </s:iterator>         
					</div>
                    </td>
                </tr>
                <tr>
                	<td width="100" height="46" style="color: red">*公司地址：</td>
                	<td width="864">
                    	<input name="bo.com_address" id="txt_com_address" value="${bo.com_address}" maxlength="100" type="text" style="width:719px; padding-left:5px;height:33px; line-height:33px; background:url(<%=basePath %>enterprise/info/images/juchang.jpg);"/>
                    </td>
                </tr>
                <tr>
                	<td width="100" height="46" style="color: red">*联系电话：</td>
                	<td width="864">
                    	<input name="bo.com_phone" id="txt_com_phone" value="${bo.com_phone }" maxlength="50" type="text" style="width:183px; padding-left:5px;height:33px; line-height:33px; background:url(<%=basePath %>enterprise/info/images/juchang.jpg);"/>
                    </td>
                </tr>
                <tr>
                	<td width="100" height="46">公司主页：</td>
                	<td width="864">
                    	<input name="bo.com_url" id="txt_com_url" value="${bo.com_url}" type="text" style="width:432px; padding-left:5px;height:33px; line-height:33px; background:url(<%=basePath %>enterprise/info/images/juchang.jpg);"/>
                    </td>
                </tr>
                <tr>
                	<td width="100" height="130" style="vertical-align:top; padding-top:10px;">公司简介：</td>
                	<td width="864">
                    	<textarea name="bo.com_info" id="txt_com_info" cols="" rows="" style="width:714px; height:110px; padding:5px; background:url(<%=basePath %>enterprise/info/images/ta.jpg); overflow:hidden;">${bo.com_info}</textarea>
                    </td>
                </tr>
                <tr>
                	<td width="100" height="80"></td>
                	<td width="864">
                    	<input name="" id="btn_save" type="button" value="保存" style="width:190px; height:46px; background:url(<%=basePath %>enterprise/info/images/fb.jpg); font-size:16px; color:#fdfdfd; cursor:pointer;"/>
                        <input name="" id="btn_cancel" type="button" value="取消" style="width:190px; height:46px; background:url(<%=basePath %>enterprise/info/images/bc.jpg); font-size:16px; color:#747171; margin-left:30px;cursor:pointer;"/>
                     	
                    </td>
                </tr>
            </table>
        </div>
      </div>
      </form>
      <s:include value="../../common/page/footer.jsp"></s:include>
    </div>
                  <!-- alpha div -->
<div id="maskLayer" style="display:none">
<iframe id="maskLayer_iframe" frameBorder=0 scrolling=no style="filter:alpha(opacity=50)"></iframe>
<div id="alphadiv" style="filter:alpha(opacity=50);-moz-opacity:0.5;opacity:0.5"></div>
	<div id="drag">
		<h3 id="drag_h"></h3>
		<div id="drag_con"></div><!-- drag_con end -->
	</div>
</div><!-- maskLayer end -->
</div>
<!-- alpha div end -->
<div id="sublist" style="display:none"></div>
</body>
</html>
